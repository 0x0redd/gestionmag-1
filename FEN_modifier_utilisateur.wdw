   -
     name : RemplirComboRoles
     procedure_id : 1744547090690484020
     type_code : 14
     code : |1+
      PROCÉDURE RemplirComboRoles()
      SI HExécuteRequête(REQ_Liste_Roles) = Faux ALORS
      	Info("Erreur : " + HErreurInfo())
      	RETOUR
      FIN
      
      
      
     type : 458752
   -
     name : InitModification
     procedure_id : 1744547584613869598
     type_code : 14
     code : |1+
      PROCÉDURE InitModification(idUser)
      SI HLitRecherche(users, id, idUser) = Faux ALORS
      	Info("Utilisateur introuvable.")
      	RETOUR
      FIN
      
      // Récupération des infos liées
      HLitRecherche(people, id, users.person_id)
      HLitRecherche(role_user, user_id, users.id)
      HLitRecherche(roles, id, role_user.role_id)
      
      // Remplir les champs
      EDT_Nom			= people.last_name
      EDT_Prenom		= people.first_name
      SAI_Email		= users.email
      EDT_Telephone	= people.phone_number
      EDT_Adresse		= people.address_1
      EDT_Ville		= people.city
      EDT_Pays		= people.country
      SAI_Gender		= people.gender
      
      // 🟢 Sélection du rôle dans le ComboBox APRÈS qu'il soit rempli
      COMBO_Role = role_user.role_id
      
      
      // Stocker l'ID pour modification
      Glo_idUser		= users.id
      Glo_idPerson	= people.id
      
      
