  p_codes :
   -
     code : |1-
      PROCÉDURE MaFenêtre(idUserParam est un entier)
      Glo_idUser				est un entier	= idUserParam
      Glo_idPerson			est un entier
      Glo_modificationFaite	est un booléen	= Faux
      MaValeurRetour			est un booléen	= Faux
      SI HLitRecherche(users, id, Glo_idUser) ALORS
      	Glo_idPerson = users.person_id
      	
      	SI HLitRecherche(people, id, Glo_idPerson) ALORS
      		EDT_Prenom		= people.first_name
      		EDT_Nom			= people.last_name
      		SAI_Gender		= people.gender
      		EDT_Telephone	= people.phone_number
      		EDT_Adresse		= people.address_1
      		EDT_Ville		= people.city
      		EDT_Pays		= people.country
      		SAI_Email		= people.email
      	FIN
      FIN
   -
     code : |1+
      
      
      
      // Stocker l'ID utilisateur passé en paramètre
      Glo_idUser = idUserParam
      
      // 1. Charger les rôles dans le ComboBox
      RemplirComboRoles()
      
      // 2. Charger les données de l'utilisateur et sélectionner son rôle
      InitModification(Glo_idUser)
