  -
    name : BTN_Modifier
    identifier : 0x282cb3bf4cfb6b8c
    internal_properties : 
    properties :
     x : 105
     y : 347
     width : 144
     height : 33
     accelerator_key : 65535
     tab_order : 10
    style : {}
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
        type : 14
      -
        code : |1+
         // 1. Modifier people
         REQ_modifier_people.Paramfirst_name		= EDT_Prenom
         REQ_modifier_people.Paramlast_name		= EDT_Nom
         REQ_modifier_people.Paramgender			= SAI_Gender
         REQ_modifier_people.Paramphone_number	= EDT_Telephone
         REQ_modifier_people.Paramaddress_1		= EDT_Adresse
         REQ_modifier_people.Paramcity			= EDT_Ville
         REQ_modifier_people.Paramcountry		= EDT_Pays
         REQ_modifier_people.Paramemail			= SAI_Email
         REQ_modifier_people.Paramcreated_at		= DateSys()
         REQ_modifier_people.Paramid				= Glo_idPerson
         
         SI HExécuteRequête(REQ_modifier_people) = Faux ALORS
         	Info("Erreur sur people : " + HErreurInfo())
         	RETOUR
         FIN
         
         
         
         // 2. Modifier users
         REQ_modifier_user.Paramemail	= SAI_Email
         REQ_modifier_user.Paramname		= EDT_Prenom + " " + EDT_Nom
         REQ_modifier_user.Paramid		= Glo_idUser
         
         SI HExécuteRequête(REQ_modifier_user) = Faux ALORS
         	Info("Erreur sur users : " + HErreurInfo())
         	RETOUR
         FIN
         
         // 3. Modifier le mot de passe si un nouveau est saisi
         SI EDT_Password <> "" ALORS
         	REQ__modifier_password.Parampassword	= EDT_Password
         	REQ__modifier_password.Paramid			= Glo_idUser
         
         	SI HExécuteRequête(REQ__modifier_password) = Faux ALORS
         		Info("Erreur lors de la modification du mot de passe : " + HErreurInfo())
         		RETOUR
         	FIN
         FIN
         
         
         // 3. Modifier role_user
         roleId est un entier
         
         SELON COMBO_Role..ValeurAffichée
         	CAS "SuperAdmin"
         		roleId = 1
         	CAS "Responsable"
         		roleId = 2
         	CAS "Magasinier"
         		roleId = 3
         	AUTRE CAS
         		Info("Rôle invalide.")
         		RETOUR
         FIN
         
         // Assigner les paramètres à la requête
         REQ_modifier_role_user.Paramrole_id	= roleId
         REQ_modifier_role_user.Paramuser_id	= Glo_idUser
         
         SI HExécuteRequête(REQ_modifier_role_user) = Faux ALORS
         	Info("Erreur lors de la modification du rôle : " + HErreurInfo())
         	RETOUR
         FIN
         
         Info("Utilisateur modifié avec succès.")
         MaValeurRetour = Vrai
         Ferme()
        type : 18
