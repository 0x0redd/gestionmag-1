         // Modification de people
         SI HLitRecherche(people, id, Glo_idPerson) ALORS
         	people.first_name	= EDT_Prenom
         	people.last_name	= EDT_Nom
         	people.gender		= SAI_Gender
         	people.phone_number	= EDT_Telephone
         	people.address_1	= EDT_Adresse
         	people.city			= EDT_Ville
         	people.country		= EDT_Pays
         	people.email		= SAI_Email
         	SI HModifie(people) = Faux ALORS
         		Info("Erreur lors de la modification de la table people : " + HErreurInfo())
         		RETOUR
         	FIN
         
         FIN
         
         // Modification de users
         SI HLitRecherche(users, id, Glo_idUser) ALORS
         	users.email	= SAI_Email
         	users.name	= EDT_Prenom + " " + EDT_Nom
         	SI HModifie(people) = Faux ALORS
         		Info("Erreur lors de la modification de la table people : " + HErreurInfo())
         		RETOUR
         	FIN
         
         FIN
         roleId est un entier
         SI COMBO_Role..ValeurAffichée = "SuperAdmin" ALORS
         	roleId = 1
         SINON SI COMBO_Role..ValeurAffichée = "Responsable" ALORS
         	roleId = 2
         SINON SI COMBO_Role..ValeurAffichée = "Magasinier" ALORS
         	roleId = 3
         SINON
         	Info("Rôle invalide sélectionné.")
         	RETOUR
         FIN
         
         
         
         
         
         
         SI HLitRecherche(role_user, user_id, Glo_idUser) ALORS
         	role_user.role_id = roleId
         	SI HModifie(people) = Faux ALORS
         		Info("Erreur lors de la modification de la table people : " + HErreurInfo())
         		RETOUR
         	FIN
         
         FIN
         
         Info("Modification réussie !")
         Glo_modificationFaite = Vrai
         // Si tu veux que la table se recharge dans le dashboard
         FEN_SuperAdmin.ChargerUtilisateurs()
         MaValeurRetour est un booléen = Faux
         
         
         Ferme()
         
