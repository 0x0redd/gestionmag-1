  procedures :
   -
     name : InitMode
     procedure_id : 1739416803762741994
     type_code : 14
     code : |1+
      PROCÉDURE InitMode(mode est une chaîne)
      
      SELON mode
      	CAS "voir"
      		AfficherPermissions()
      		BTN_Valider..Visible = Faux
      		
      	CAS "ajouter"
      		ChargerComboPermissionsDisponibles()
      		BTN_Valider..Caption	= "Ajouter"
      		BTN_Valider..Visible	= Vrai
      		
      	CAS "modifier"
      		ChargerPermissionsExistantPourModifier()
      		BTN_Valider..Caption	= "Modifier"
      		BTN_Valider..Visible	= Vrai
      		
      	CAS "supprimer"
      		AfficherPermissions()
      		BTN_Valider..Caption	= "Supprimer"
      		BTN_Valider..Visible	= Vrai
      FIN
      
      
     type : 458752
   -
     name : AfficherPermissions
     procedure_id : 1739419629851553519
     type_code : 14
     code : |1+
      PROCÉDURE AfficherPermissions()
      roleId est un entier = ChercherIdRole(COMBO_Role..ValeurAffichée)
      SI roleId = 0 ALORS RETOUR
      
      REQ_voir_permissions.Paramrole_id = roleId
      SI HExécuteRequête(REQ_voir_permissions) ALORS
      	TABLE_Permissions..Source = REQ_voir_permissions
      SINON
      	Info("Erreur lors de la récupération des permissions : " + HErreurInfo())
      FIN
      
     type : 458752
   -
     name : ChercherIdRole
     procedure_id : 1739421145975033937
     type_code : 14
     code : |1+
      PROCÉDURE ChercherIdRole(nom est une chaîne)
      SI HLitPremier(roles) ALORS
      	TANT_QUE PAS HEnDehors()
      	SI roles.name = nom ALORS
      		RETOUR roles.id
      	FIN
      	HLitSuivant(roles)
      FIN
      FIN
      RETOUR 0
      
      
     type : 458752
