  -
    name : BTN_Valider
    identifier : 0x282e610729e1c441
    internal_properties : 
    properties :
     x : 812
     y : 623
     width : 109
     height : 32
     anchor : 4
     accelerator_key : 65535
     tab_order : 5
    style : {}
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
        type : 14
      -
        code : |1+
         // Vérification des champs obligatoires
         SI EDT_NomArticle = "" ALORS
         	Erreur("Veuillez saisir un nom d'article.")
         	RETOUR
         FIN
         
         SI SAI_categorie = "" ALORS
         	Erreur("Veuillez saisir une catégorie.")
         	RETOUR
         FIN
         
         SI EDT_PrixAchat <= 0 ALORS
         	Erreur("Veuillez saisir un prix d'achat valide.")
         	RETOUR
         FIN
         
         SI EDT_PrixVente <= 0 ALORS
         	Erreur("Veuillez saisir un prix de vente valide.")
         	RETOUR
         FIN
         
         SI EDT_Description = "" ALORS
         	Erreur("Veuillez saisir une description.")
         	RETOUR
         FIN
         
         // Affectation des paramètres de la requête d'insertion
         
         REQ_update_itmsY.pitems_number	= "ART-" + NumériqueVersChaîne(HeureSys(), "000000")
         REQ_update_itmsY.pname			= EDT_NomArticle
         REQ_update_itmsY.pcategory		= SAI_categorie
         REQ_update_itmsY.pcost_price		= EDT_PrixAchat
         REQ_update_itmsY.punit_price		= EDT_PrixVente
         REQ_update_itmsY.pdescription	= EDT_Description
         REQ_update_itmsY.pstock_type		= null
         REQ_update_itmsY.pcreated_at		= DateSys()
         REQ_update_itmsY.pid=id
         
         
         
         // Traitement de la photo - NOUVELLE APPROCHE: stocker le chemin plutôt que l'image
         SI IMG <> "" ALORS
         	// Stocker directement le chemin de l'image
         	REQ_update_itmsY.ppictures = IMG
         SINON
         	REQ_update_itmsY.ppictures = Null
         FIN
         
         // Exécution de la requête d'insertion
         SI HExécuteRequête(REQ_update_itmsY) = Faux ALORS
         	Erreur("Erreur lors de l'ajout de l'article : " + HErreurInfo())
         	RETOUR
         FIN
         
         // Réinitialisation des champs
         EDT_NomArticle	= ""
         SAI_categorie	= ""
         EDT_PrixAchat	= ""
         EDT_PrixVente	= ""
         EDT_Description	= ""
         IMG	= ""
         
         // Message de confirmation
         Info("Article est modifiee avec succès !")
         MaFenêtre.close()
         
        type : 18
