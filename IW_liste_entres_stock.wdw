        type_code : 4
        p_codes :
         -
           code : |1+
            // Nom du fichier PDF avec date/heure
            sNomFichier		est une chaîne	= "Facture_" + DateVersChaîne(DateSys(), "AAAAMMJJ_HHMMSS") + "_" + Hasard(1000, 9999) + ".pdf"
            
            
            sCheminFichier	est une chaîne	= fRepDonnées() + "\" + sNomFichier
            
            // Définir la destination : PDF
            iDestination(iPDF, sCheminFichier)
            
            // Bloc d’exception pour capturer les erreurs d’impression
            QUAND EXCEPTION DANS
            	iImprimeEtat("ETAT_FAC_ENTR_Stock") // mets le nom exact de ton état
            FAIRE
            	Info("Erreur lors de l'impression de l'état : " + ExceptionInfo())
            	RETOUR
            FIN
            
            // Attendre un peu (pour laisser le temps au fichier de se générer)
            Temporisation(200)
            
            // Vérifier si le fichier a bien été généré
            SI fFichierExiste(sCheminFichier) ALORS
            	Info("Facture générée : " + sCheminFichier)
            	fOuvre(sCheminFichier)
            SINON
            	Info("La facture n'a pas pu être générée.")
            FIN
