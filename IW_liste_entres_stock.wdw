     -
       name : BTN_SansNom1
       identifier : 0x2853626a3b10ea1b
       internal_properties : 
       properties :
        x : 1035
        y : 23
        width : 126
        height : 56
        anchor : 4
        accelerator_key : 65535
        tab_order : 3
       style : {}
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1-
            // Bouton Supprimer - Code de suppression en cascade
            
            // 1. Vérifier qu'une ligne est sélectionnée
            SI TableSelect(TABLE_Liste_Art) = -1 ALORS
            	Erreur("Veuillez sélectionner une ligne à supprimer.")
            	RETOUR
            FIN
            
            // 2. Récupérer les informations de la ligne sélectionnée
            nLigneSelectionnee	est un entier	= TableSelect(TABLE_Liste_Art)
            
            // Récupérer les IDs nécessaires depuis la table (adapter les noms de colonnes selon votre table)
            nReceivingID		est un entier	= TABLE_Liste_Art[nLigneSelectionnee].COL_ID
            nItemID				est un entier	= TABLE_Liste_Art[nLigneSelectionnee]
            nLocationID			est un entier	= TABLE_Liste_Art[nLigneSelectionnee]
            
            // 3. Demander confirmation avant suppression
            SI OuiNon("Êtes-vous sûr de vouloir supprimer cette réception ?", "Cette action supprimera toutes les données associées et ne peut pas être annulée.") = Non ALORS
            	RETOUR
            FIN
            
            // 4. Suppression en cascade (dans l'ordre inverse des dépendances)
            
            // 4.1 Supprimer les paiements associés
            REQ_supprime_payment.Paramreceivings_id = nReceivingID
            
            SI PAS HExécuteRequête(REQ_supprime_payment) ALORS
            	Erreur("Erreur lors de la suppression des paiements : " + HErreurInfo())
            	RETOUR
            FIN
            
            // 4.2 Supprimer les items de réception
            REQ_supprime_recieving_items.Paramreceivings_id = nReceivingID
            
            SI PAS HExécuteRequête(REQ_supprime_recieving_items) ALORS
            	Erreur("Erreur lors de la suppression des items de réception : " + HErreurInfo())
            	RETOUR
            FIN
            
            // 4.3 Supprimer les quantités d'items en stock
            REQ_supprime_items_quantities.Paramitem_id		= nItemID
            REQ_supprime_items_quantities.Paramlocation_id	= nLocationID
            
            SI PAS HExécuteRequête(REQ_supprime_items_quantities) ALORS
            	Erreur("Erreur lors de la suppression des quantités : " + HErreurInfo())
            	RETOUR
            FIN
            
            // 4.4 Supprimer la réception principale
            REQ_supprimer_receiving.ParamId = nReceivingID
            
            SI PAS HExécuteRequête(REQ_supprimer_receiving) ALORS
            	Erreur("Erreur lors de la suppression de la réception : " + HErreurInfo())
            	RETOUR
            FIN
            
            // 5. Actualiser la table et informer l'utilisateur
            TableSupprime(TABLE_Liste_Art, nLigneSelectionnee)
            Info("Réception supprimée avec succès.")
            
            // 6. Optionnel : Recharger les données depuis la base
             HExécuteRequête(REQ_selection_entr_stock)
             TableAffiche(TABLE_Liste_Art, taInit)
