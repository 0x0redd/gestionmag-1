           code : |1-
            
           type : 14
         -
           code : |1-
            nItemID			est un entier	= COMBO_article
            nLocationID		est un entier	= COMBO_stock
            nCustomerID		est un entier	= COMBO_clients
            
            // Vérification des champs
            SI nItemID = 0 OU nLocationID = 0 OU nCustomerID = 0 ALORS
            	Erreur("Veuillez sélectionner un article, un stock et un client.")
            	RETOUR
            FIN
            
            SI Val(Quantitée) > Val(SAI_QteTotal) ALORS
            	INFO(Quantitée , SAI_QteTotal)
            	Erreur("le stock est insufissant")
            	RETOUR
            FIN
            
            
            
            // Récupération de la quantité
            nQuantite est un entier = Val(Quantitée)
            SI nQuantite <= 0 ALORS
            	Erreur("Veuillez saisir une quantité valide.")
            	RETOUR
            FIN
            
            // Préparation des données
            sCommentaire		est une chaîne	= EDT_Commentaire
            sInvoiceNumber		est une chaîne	= "FACT-" + NumériqueVersChaîne(HeureSys(), "0")
            sDate				est une chaîne	= DateVersChaîne(DateSys(), "AAAA-MM-JJ HH:MM:SS")
            nUserID				est un entier	= 1	// à adapter
            
            reference = sInvoiceNumber
            
            // Insertion de la vente (une seule fois !)
            REQ_ajout_sale_insert.Paramcustomer_id			= nCustomerID
            REQ_ajout_sale_insert.Paramcommentaire			= sCommentaire
            REQ_ajout_sale_insert.Paraminvoice_number		= sInvoiceNumber
            REQ_ajout_sale_insert.Paramsale_status			= "en cours"
            REQ_ajout_sale_insert.Paramlocation_id			= nLocationID
            REQ_ajout_sale_insert.ParamsalePayment_status	= "non payé"
            REQ_ajout_sale_insert.Paramcreated_at			= sDate
            REQ_ajout_sale_insert.Paramuser_id				= nUserID
            
            SI PAS HExécuteRequête(REQ_ajout_sale_insert) ALORS
            	Erreur("Erreur lors de l'ajout de la vente : " + HErreurInfo())
            	RETOUR
            FIN
            
            // Récupération de l'ID de la vente créée
            SI PAS HExécuteRequête(REQ_select_max_id_sales) ALORS
            	Erreur("Impossible de récupérer l'ID de la vente : " + HErreurInfo())
            	RETOUR
            FIN
            
            SI HLitPremier(REQ_select_max_id_sales) = Faux ALORS
            	Erreur("Aucune vente trouvée.")
            	RETOUR
            FIN
            
            nSaleID est un entier = REQ_select_max_id_sales.le_maximum_id
            
            // Récupération des prix
            REQ_select_prices.Paramid = nItemID
            SI PAS HExécuteRequête(REQ_select_prices) ALORS
            	Erreur("Impossible de récupérer les prix de l'article : " + HErreurInfo())
            	RETOUR
            FIN
            
            HLitPremier(REQ_select_prices)
            nCostPrice	est un réel	= REQ_select_prices.cost_price
            nUnitPrice	est un réel	= REQ_select_prices.unit_price
            
            // Insertion dans sales_items
            REQ_Ajout_sales_items.Paramsale_id				= nSaleID
            REQ_Ajout_sales_items.Paramitem_id				= nItemID
            REQ_Ajout_sales_items.Paramdescription			= "Article vendu"
            REQ_Ajout_sales_items.Paramserialnumber			= ""
            REQ_Ajout_sales_items.Paramquantity_purchased	= nQuantite
            REQ_Ajout_sales_items.Paramitem_cost_price		= nCostPrice
            REQ_Ajout_sales_items.Paramitem_unit_price		= nUnitPrice
            REQ_Ajout_sales_items.Paramitem_location		= nLocationID
            REQ_Ajout_sales_items.Paramcreated_at			= sDate
            
            SI PAS HExécuteRequête(REQ_Ajout_sales_items) ALORS
            	Erreur("Erreur lors de l'ajout des articles : " + HErreurInfo())
            	RETOUR
            FIN
            Total_paiement								= total_achat_cal
            
            // Affectation des paramètres de la requête
            REQ_Ajout_sales_items.Paramitem_id			= nItemID
            REQ_Ajout_sales_items.Paramitem_location	= nLocationID
            
            // Exécution de la requête
            SI PAS HExécuteRequête(REQ_select_items_quantites_md) ALORS
            	Erreur("Erreur lors de la récupération de la quantité : " + HErreurInfo())
            	RETOUR
            FIN
            
            // Lecture du premier résultat
            SI HLitPremier(REQ_select_items_quantites_md) = Faux ALORS
            	Erreur("Article introuvable dans les stocks.")
            	RETOUR
            FIN
            
            // Lecture de la quantité actuelle
            nAncienneQuantite	est un entier	= REQ_select_items_quantites_md.quantity
            
            // Calcul de la nouvelle quantité
            nNouvelleQuantite	est un entier	= SAI_QteTotal - Quantitée
            
            
            
            // Mise à jour
            
            
            REQ_modifier_items_quantities.Paramquantity		= nNouvelleQuantite
            
            SI PAS HExécuteRequête(REQ_modifier_items_quantities) ALORS
            	Erreur("Erreur lors de la mise à jour des quantités : " + HErreurInfo())
            	RETOUR
            FIN
            
            Info("Vente et mise à jour du stock effectuées avec succès.")
            SAI_QteTotal = nNouvelleQuantite
