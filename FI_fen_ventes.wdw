            nItemID est un entier = COMBO_article
            sale_id est un entier =  HExécuteRequête(REQ_select_max_id_sales)
            
            // --- Insertion dans sales_payments selon le mode de paiement ---
            SI espece_saisie <> "" ALORS
            	// Paiement en espèce
            	REQ_sales_payments.Parampayment_type		= "Espèce"
            	REQ_sales_payments.Parampayment_amount	= espece_saisie
            	REQ_sales_payments.Paramsale_id		= sale_id
            	REQ_sales_payments.Paramdate_cheque		= Null
            	REQ_sales_payments.Paramcreated_at		= DateSys()
            
            	SI HExécuteRequête(REQ_sales_payments) = Faux ALORS
            		Erreur("Erreur lors du paiement en espèce : " + HErreurInfo())
            		RETOUR
            	FIN
            FIN
            
            SI cheque_saisie <> "" ALORS
            	// Paiement par chèque
            	REQ_sales_payments.Parampayment_type		= "Chèque"
            	REQ_sales_payments.Parampayment_amount	= cheque_saisie
            	REQ_sales_payments.Paramsale_id		= HExécuteRequête(REQ_select_max_id_sales)
            	REQ_sales_payments.Paramdate_cheque		= SAI_Date_cheque
            	REQ_sales_payments.Paramcreated_at		= DateSys()
            
            	SI HExécuteRequête(REQ_sales_payments) = Faux ALORS
            		Erreur("Erreur lors du paiement par chèque : " + HErreurInfo())
            		RETOUR
            	FIN
            FIN
            
            SI carte_saisie <> "" ALORS
            	// Paiement par carte
            	REQ_sales_payments.Parampayment_type		= "Carte"
            	REQ_sales_payments.Parampayment_amount	= carte_saisie
            	REQ_sales_payments.Paramsale_id		= sale_id
            	REQ_sales_payments.Paramdate_cheque		= Null
            	REQ_sales_payments.Paramcreated_at		= DateSys()
            
            	SI HExécuteRequête(REQ_sales_payments) = Faux ALORS
            		Erreur("Erreur lors du paiement par carte : " + HErreurInfo())
            		RETOUR
            	FIN
            FIN
            
            Info("Réception et paiement enregistrés avec succès.")
