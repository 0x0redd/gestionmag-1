        code : |1+
         // Récupérer l'ID du produit sélectionné (valeur mémorisée)
         idArticle est un entier = COMBO_article.ValeurMémorisée
         
         // Vérifier que l'ID est valide
         SI idArticle <> 0 ALORS
         	// Aller lire l'article correspondant
         	SI HLitRecherche(REQ_Select_Article_YY, id, idArticle) ALORS
         		// Afficher le nom et le prix dans les libellés
         		Produit_name	= REQ_Select_Article_YY.name
         		prix_produit	= REQ_Select_Article_YY.cost_price
         	SINON
         		Info("Article introuvable")
         	FIN
         	
         	REQ_get_stock.produitID = idArticle
         	SI PAS HExécuteRequête(REQ_get_stock) ALORS
         		Erreur("Erreur lors de la lecture du stock : " + HErreurInfo())
         		RETOUR
         	FIN
         	
         	SI HLitPremier(REQ_get_stock) ALORS
         		SAI_QteTotal = REQ_get_stock.quantity
         	SINON
         		Info("Aucune donnée de stock trouvée pour cet article.")
         	FIN
         	
         	
         SINON
         	Info("Aucun article sélectionné")
         FIN
         
