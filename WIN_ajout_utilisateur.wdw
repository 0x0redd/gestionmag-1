  -
    name : BTN_CreerCompte
    identifier : 0x2817af7e07d802fa
    internal_properties : 
    properties :
     x : 75
     y : 431
     width : 125
     height : 37
     accelerator_key : 65535
     tab_order : 10
    style : {}
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
        type : 14
      -
        code : |1+
         // 🔍 Vérifier si l'e-mail existe déjà dans USERS
         HAnnuleRecherche(users)
         HRAZ(users)
         SI HLitRecherche(users, users.email, SAI_Email) ALORS
         	Info("Un utilisateur avec cet e-mail existe déjà.")
         	RETOUR
         FIN
         
         // 🧼 Insertion dans la table people
         HReset(people)
         people.first_name	= EDT_Prenom
         people.last_name	= EDT_Nom
         people.gender		= SAI_Gender
         people.phone_number	= EDT_Telephone
         people.email		= SAI_Email
         people.address_1	= EDT_Adresse
         people.city			= EDT_Ville
         people.country		= EDT_Pays
         people.created_at	= DateSys()
         
         SI HAjoute(people) = Faux ALORS
         	Info("Erreur lors de l’ajout dans la table people.")
         	RETOUR
         FIN
         
         // 🔁 Récupération de l'ID de la personne ajoutée
         SI HLitDernier(people) = Faux ALORS
         	Info("Impossible de récupérer l’ID de la personne.")
         	RETOUR
         FIN
         
         // 🔒 Vérification mot de passe
         SI EDT_Password = "" ALORS
         	Info("Veuillez saisir un mot de passe.")
         	RETOUR
         FIN
         
         // Ajout dans la table users
         HReset(users)
         users.name			= EDT_Prenom + " " + EDT_Nom
         users.email			= SAI_Email
         users.password		= EDT_Password
         users.created_at	= DateSys()
         users.person_id		= people.id
         
         SI HAjoute(users) = Faux ALORS
         	Info("Erreur lors de l’ajout dans la table users.")
         	RETOUR
         FIN
         
         // ✅ Récupération fiable de l’ID utilisateur
         idUser	est un entier	= users.id
         
         // ✅ Déterminer l'ID du rôle sélectionné
         roleId	est un entier
         SI COMBO_Role..ValeurAffichée = "SuperAdmin" ALORS
         	roleId = 1
         SINON SI COMBO_Role..ValeurAffichée = "Responsable" ALORS
         	roleId = 2
         SINON SI COMBO_Role..ValeurAffichée = "Magasinier" ALORS
         	roleId = 3
         SINON
         	Info("Rôle invalide sélectionné.")
         	RETOUR
         FIN
         
         // 🔍 Vérifier que le rôle existe
         SI PAS HLitRecherche(roles, roles.id, roleId) ALORS
         	Info("Ce rôle n'existe pas.")
         	RETOUR
         FIN
         
         // ❌ Vérifier si ce user a déjà un rôle
         SI HLitRecherche(role_user, "user_id", idUser) ALORS
         	Info("Ce compte a déjà un rôle attribué.")
         	RETOUR
         FIN
         
         // ✅ Insertion dans role_user
         HReset(role_user)
         role_user.user_id	= idUser
         role_user.role_id	= roleId
         
         SI HAjoute(role_user) = Faux ALORS
         	Info("Erreur lors de l’attribution du rôle.")
         	RETOUR
         FIN
         
         // 🎉 Succès
         Info("Utilisateur ajouté avec succès.")
         Ferme()
         
         
