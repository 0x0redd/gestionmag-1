        code : |1+
         
         // Fonction qui se déclenche lorsque l'utilisateur saisit la quantité
         PROCÉDURE CalculerTotal()
         nDifference est un entier
         // Calcul du total
         qte				est un entier	= Val(SAISIE_Quantite)
         prix			est un réel		= Val(SAISIE_Cost_Price)
         total_achat		est un réel		= qte * prix
         SAISIE_Montant_Paiement = total_achat
         
         // Vérifier si le stock est chargé
         SI STC_Quantite_Stock = "" ALORS
         	Info("Quantité en stock non chargée.")
         	RETOUR
         FIN
         
         // Calcul de la nouvelle quantité en stock
         nStockActuel			est un entier	= Val(STC_Quantite_Stock)
         nNouvelleQuantite		est un entier	= Val(SAISIE_Quantite)
         nNouvelleQuantiteStock	est un entier
         
         SI nNouvelleQuantite > nQuantiteInitiale ALORS
         	nDifference				= nNouvelleQuantite - nQuantiteInitiale
         	nNouvelleQuantiteStock	= nStockActuel + nDifference
         
         SINON SI nNouvelleQuantite < nQuantiteInitiale ALORS
         	nDifference				= nQuantiteInitiale - nNouvelleQuantite
         	nNouvelleQuantiteStock	= nStockActuel - nDifference
         
         SINON
         	nNouvelleQuantiteStock = nStockActuel
         FIN
         
         EDT_Nouvelle_Quantite = nNouvelleQuantiteStock
         
