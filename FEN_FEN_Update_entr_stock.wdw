   -
     code : |1+
      PROCÉDURE MaFenêtre(Receivingid, Receiving_item_id, location_id )
      nQuantiteInitiale est un entier = 0
      
      // Exécuter la requête de sélection
      REQ_selection_entr_stock_modif.paramid = Receivingid
      SI HExécuteRequête(REQ_selection_entr_stock_modif) ALORS
      	SI HLitPremier(REQ_selection_entr_stock_modif) ALORS
      		COMBO_Type_Paiement			= REQ_selection_entr_stock_modif.paymentID
      		COMBO_etat_paiement			= REQ_selection_entr_stock_modif.receivingPayment_status
      		COMBO_Fournisseur			= REQ_selection_entr_stock_modif.person_id
      		SAISIE_Montant_Paiement		= REQ_selection_entr_stock_modif.payment_amount
      		
      		SI REQ_selection_entr_stock_modif.item_location_id <> "" ALORS
      			COMBO_Stock = REQ_selection_entr_stock_modif.item_location_id
      		FIN
      		
      		SI REQ_selection_entr_stock_modif.item_id <> "" ALORS
      			COMBO_Article = REQ_selection_entr_stock_modif.item_id
      		FIN
      		
      		SI REQ_selection_entr_stock_modif.quantity_purchased <> "" ALORS
      			SAISIE_Quantite		= REQ_selection_entr_stock_modif.quantity_purchased
      			nQuantiteInitiale	= Val(REQ_selection_entr_stock_modif.quantity_purchased)
      		FIN
      		
      		SI REQ_selection_entr_stock_modif.item_cost_price <> "" ALORS
      			SAISIE_Cost_Price = REQ_selection_entr_stock_modif.item_cost_price
      		FIN
      		
      		SI REQ_selection_entr_stock_modif.payment_type <> "" ALORS
      			nIndice est un entier = ListeCherche(COMBO_Type_Paiement, REQ_selection_entr_stock_modif.payment_type)
      			SI nIndice > 0 ALORS
      				COMBO_Type_Paiement = nIndice
      			SINON
      				COMBO_Type_Paiement = 1
      			FIN
      		FIN
      	SINON
      		Info("Aucun enregistrement trouvé.")
      		Ferme()
      		RETOUR
      	FIN
      SINON
      	Erreur("Erreur REQ_selection_entr_stock_modif : " + HErreurInfo())
      	Ferme()
      	RETOUR
      FIN
      
      // Chargement du nom du stock s’il n’est pas encore affecté
      SI COMBO_Stock = "" ALORS
      	REQ_select_stocklocation_modification.Paramid = location_id
      	SI HExécuteRequête(REQ_select_stocklocation_modification) ALORS
      		SI HLitPremier(REQ_select_stocklocation_modification) ALORS
      			COMBO_Stock	= REQ_select_stocklocation_modification.id
      		SINON
      			Info("Stock introuvable.")
      		FIN
      	SINON
      		Erreur("Erreur exécution REQ_select_stocklocation_modification : " + HErreurInfo())
      		RETOUR
      	FIN
      FIN
      
      // S’il manque infos sur article ou quantité, compléter via receiving_items
      SI COMBO_Article = "" OU SAISIE_Quantite = "" ALORS
      	REQ_selection_receivingItems.Paramreceivings_id = Receivingid
      	SI HExécuteRequête(REQ_selection_receivingItems) ALORS
      		SI HLitPremier(REQ_selection_receivingItems) ALORS
      			SI COMBO_Article = "" ALORS
      				COMBO_Article = REQ_selection_receivingItems.item_id
      			FIN
      			SI SAISIE_Quantite = "" ALORS
      				SAISIE_Quantite = REQ_selection_receivingItems.quantity_purchased
      			FIN
      			SI SAISIE_Montant_Paiement = "" ALORS
      				SAISIE_Montant_Paiement = REQ_selection_receivingItems.item_cost_price
      			FIN
      		FIN
      	SINON
      		Erreur("Erreur REQ_selection_receivingItems : " + HErreurInfo())
      		RETOUR
      	FIN
      FIN
      
      // Charger la quantité actuelle du stock
      REQ_total_quantity.param_item_id		= Receiving_item_id
      REQ_total_quantity.patam_location_id	= location_id
      
      SI HExécuteRequête(REQ_total_quantity) ALORS
      	SI HLitPremier(REQ_total_quantity) ALORS
      		STC_Quantite_Stock = REQ_total_quantity.quantity
      	SINON
      		STC_Quantite_Stock = 0
      		Info("Aucune quantité en stock trouvée.")
      	FIN
      SINON
      	Erreur("Erreur chargement quantité réelle : " + HErreurInfo())
      	RETOUR
      FIN
