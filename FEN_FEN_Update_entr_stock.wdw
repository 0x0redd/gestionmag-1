     p_codes :
      -
        code : |1-
         
        type : 14
      -
        code : |1+
         // =========================================
         // 1. Lecture des champs de l'interface
         // =========================================
         nReceivingID		est un entier	= Receivingid
         nItemID				est un entier	= COMBO_Article..Valeur
         nStockID			est un entier	= COMBO_Stock..Valeur
         nFournisseurID		est un entier	= COMBO_Fournisseur..Valeur
         sPaymentStatus		est une chaîne	= COMBO_etat_paiement..ValeurAffichée
         nQuantite			est un entier	= Val(SAISIE_Quantite)
         rPriceArticle		est un réel		= Val(SAISIE_Cost_Price)
         nPaymentType		est une chaîne	= COMBO_Type_Paiement..ValeurAffichée
         rMontantPaiement	est un réel		= Val(SAISIE_Montant_Paiement)
         nNouvelleQuantite	est un entier	= Val(EDT_Nouvelle_Quantite)
         sDescription est une chaîne = COMBO_Article..ValeurAffichée
         
         
         Info("Receiving ID : " + nReceivingID + RC + ...
         "Item ID : " + nItemID + RC + ...
         "Stock ID : " + nStockID + RC + ...
         "Fournisseur ID : " + nFournisseurID + RC + ...
         "État paiement : " + sPaymentStatus + RC + ...
         "Quantité : " + nQuantite + RC + ...
         "Prix : " + rPriceArticle + RC + ...
         "Type paiement : " + nPaymentType + RC + ...
         "Montant paiement : " + rMontantPaiement + RC + ...
         "Nouvelle quantité stock : " + nNouvelleQuantite
         "description" + sDescription
         )
         
         
         // =========================================
         // 2. Validation des données obligatoires
         // =========================================
         SI nReceivingID = 0 OU nItemID = 0 OU nStockID = 0 ALORS
         	Erreur("Veuillez remplir tous les champs obligatoires.")
         	RETOUR
         FIN
         
         SI nQuantite <= 0 ALORS
         	Erreur("La quantité achetée doit être > 0.")
         	RETOUR
         FIN
         
         SI nNouvelleQuantite < 0 ALORS
         	Erreur("La nouvelle quantité de stock ne peut pas être négative.")
         	RETOUR
         FIN
         
         // =========================================
         // 3. Confirmation utilisateur
         // =========================================
         SI OuiNon("Confirmer la modification de l'entrée de stock ?") = Faux ALORS
         	RETOUR
         FIN
         
         // =========================================
         // 4. Début de transaction
         // =========================================
         HTransactionDébut()
         
         // =========================================
         // 5. Mise à jour receiving_items
         // =========================================
         REQ_update_receiving_items.paramreceivings_id			= nReceivingID
         REQ_update_receiving_items.paramitem_id					= nItemID
         REQ_update_receiving_items.paramitem_location			= nStockID
         REQ_update_receiving_items.paramquantity_purchased		= nQuantite
         REQ_update_receiving_items.paramreceiving_quantity		= nQuantite
         REQ_update_receiving_items.paramcost_price				= rPriceArticle
         REQ_update_receiving_items.paramdescription				= sDescription  // si tu as un champ pour la description
         
         SI PAS HExécuteRequête(REQ_update_receiving_items) ALORS
         	HTransactionAnnule()
         	Erreur("Erreur lors de la mise à jour receiving_items : " + HErreurInfo())
         	RETOUR
         FIN
         
         // =========================================
         // 6. Mise à jour receivings
         // =========================================
         REQ_update_receivings.paramid			= nReceivingID
         REQ_update_receivings.ParamreceivingPayment_status	= sPaymentStatus
         REQ_update_receivings.Paramsupplier_id				= nFournisseurID
         
         SI PAS HExécuteRequête(REQ_update_receivings) ALORS
         	HTransactionAnnule()
         	Erreur("Erreur mise à jour receivings : " + HErreurInfo())
         	RETOUR
         FIN
         
         // =========================================
         // 7. Mise à jour payment_type (si fourni)
         SI nPaymentType > 0 ET rMontantPaiement > 0 ALORS
         	REQ_update_payment_type.parampayment_type	= nPaymentType
         	REQ_update_payment_type.parampayment_amount	= rMontantPaiement
         	REQ_update_payment_type.paramreceivings_id	= nReceivingID
         
         	SI PAS HExécuteRequête(REQ_update_payment_type) ALORS
         		HTransactionAnnule()
         		Erreur("Erreur mise à jour payment_type : " + HErreurInfo())
         		RETOUR
         	FIN
         FIN
         
         // =========================================
         // 8. Mise à jour item_quantities (quantité réelle saisie manuellement)
         REQ_update_quantities.Paramitem_id		= nItemID
         REQ_update_quantities.Paramlocation_id	= nStockID
         REQ_update_quantities.Paramquantity		= nNouvelleQuantite
         
         SI PAS HExécuteRequête(REQ_update_quantities) ALORS
         	HTransactionAnnule()
         	Erreur("Erreur mise à jour du stock réel : " + HErreurInfo())
         	RETOUR
         FIN
         
         // =========================================
         // 9. Valider la transaction
         // =========================================
         HTransactionFin()
         Info("Entrée de stock modifiée avec succès !")
         Ferme()
        type : 18
      -
        type : 161
        enabled : false
