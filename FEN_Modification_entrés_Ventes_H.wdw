     type_code : 4
     p_codes :
      -
        code : |1-
         
        type : 14
      -
        code : |1+
         // Récupération des IDs
         si  OuiNon("voulez vous de modifier ??")=Faux alors
         	return
         FIN
         nItemID			est un entier	= COMBO_article.ValeurMémorisée
         nLocationID		est un entier	= COMBO_stock.ValeurMémorisée
         nCustomerID		est un entier	= COMBO_clients.ValeurMémorisée
         
         // Vérification des champs
         SI nItemID = 0 OU nLocationID = 0 OU nCustomerID = 0 ALORS
         	Erreur("Veuillez sélectionner un article, un stock et un client.")
         	RETOUR
         FIN
         
         // Récupération de la quantité
         nQuantite est un entier = Val(Quantitée)
         SI nQuantite <= 0 ALORS
         	Erreur("Veuillez saisir une quantité valide.")
         	RETOUR
         FIN
         
         // Préparation des données
         sCommentaire		est une chaîne	= EDT_Commentaire
         sInvoiceNumber		est une chaîne	= "FACT-" + NumériqueVersChaîne(HeureSys(), "0")
         sDate				est une chaîne	= DateVersChaîne(DateSys(), "AAAA-MM-JJ HH:MM:SS")
         nUserID				est un entier	= 1	// à adapter
         
         // Insertion de la vente (une seule fois !)
         REQ_ModifierSales_Y.Paramcustomer_id			= nCustomerID
         REQ_ModifierSales_Y.Paramcommentaire			= sCommentaire
         REQ_ModifierSales_Y.Paraminvoice_number		= sInvoiceNumber
         REQ_ModifierSales_Y.Paramsale_status			= "en cours"
         REQ_ModifierSales_Y.Paramlocation_id			= nLocationID
         REQ_ModifierSales_Y.ParamsalePayment_status	= "non payé"
         REQ_ModifierSales_Y.Paramuser_id				= nUserID
         REQ_ModifierSales_Y.Paramid=id
         
         
         SI PAS HExécuteRequête(REQ_ModifierSales_Y) ALORS
         	Erreur("Erreur lors de modifier de la vente : " + HErreurInfo())
         	RETOUR
         FIN
         
         
         
         
         nSaleID est un entier = id
         
         // Récupération des prix
         REQ_select_prices.Paramid = nItemID
         SI PAS HExécuteRequête(REQ_select_prices) ALORS
         	Erreur("Impossible de récupérer les prix de l'article : " + HErreurInfo())
         	RETOUR
         FIN
         
         HLitPremier(REQ_select_prices)
         nCostPrice	est un réel	= REQ_select_prices.cost_price
         nUnitPrice	est un réel	= REQ_select_prices.unit_price
         
         // Insertion dans sales_items
         REQ_Update_item_Seles_Y.Paramsale_id				= nSaleID
         REQ_Update_item_Seles_Y.Paramitem_id				= ancienIdItems
         REQ_Update_item_Seles_Y.Paramdescription			= "Article vendu"
         REQ_Update_item_Seles_Y.Paramserialnumber			= ""
         REQ_Update_item_Seles_Y.Paramquantity_purchased	= nQuantite
         REQ_Update_item_Seles_Y.Paramitem_cost_price		= nCostPrice
         REQ_Update_item_Seles_Y.Paramitem_unit_price		= nUnitPrice
         REQ_Update_item_Seles_Y.Paramitem_location		= nLocationID
         REQ_Update_item_Seles_Y.Paramitem_idNew=nItemID
         
         SI PAS HExécuteRequête(REQ_Update_item_Seles_Y) ALORS
         	Erreur("Erreur lors de modifier des articles : " + HErreurInfo())
         	RETOUR
         FIN
         Total_paiement								= total_achat_cal
         
         // Affectation des paramètres de la requête
         REQ_Update_item_Seles_Y.Paramitem_id			= nItemID
         REQ_Update_item_Seles_Y.Paramitem_location	= nLocationID
         
         // Exécution de la requête
         SI PAS HExécuteRequête(REQ_select_items_quantites_md) ALORS
         	Erreur("Erreur lors de la récupération de la quantité : " + HErreurInfo())
         	RETOUR
         FIN
         
         // Lecture du premier résultat
         SI HLitPremier(REQ_select_items_quantites_md) = Faux ALORS
         	Erreur("Article introuvable dans les stocks.")
         	RETOUR
         FIN
         
         // Lecture de la quantité actuelle
         
         nAncienneQuantite	est un entier	= REQ_select_items_quantites_md.quantity
         
         // Calcul de la nouvelle quantité
         nNouvelleQuantite	est un entier	= nAncienneQuantite +ancienIdQuantite - nQuantite
         
         // Vérification du stock disponible
         SI nNouvelleQuantite < 0 ALORS
         	Erreur("Stock insuffisant ! Quantité disponible : " + nAncienneQuantite)
         	RETOUR
         FIN
         
         // Mise à jour
         
         
         REQ_modifier_items_quantities.Paramquantity		= nNouvelleQuantite
         
         SI PAS HExécuteRequête(REQ_modifier_items_quantities) ALORS
         	Erreur("Erreur lors de la mise à jour des quantités : " + HErreurInfo())
         	RETOUR
         FIN
         
         Info("Vente et mise à jour du stock effectuées avec succès.")
         MaFenêtre.close()
         
         
         
        type : 18
