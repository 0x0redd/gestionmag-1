    name : BTN_Valider
    identifier : 0x2869cd59caefcbfb
    internal_properties : 
    properties :
     x : 523
     y : 406
     width : 109
     height : 32
     accelerator_key : 65535
     tab_order : 12
    style : {}
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
        type : 14
      -
        code : |1+
         // Récupération des valeurs depuis les champs/interfaces
         nSaleID				est un entier	= SaleId
         nClientID			est un entier	= COMBO_client..Valeur
         nArticleID			est un entier	= COMBO_article..Valeur
         nStockID			est un entier	= COMBO_stock..Valeur
         nQuantiteVendue		est un entier	= Val(Quantitée)
         rPrixUnitaire		est un réel		= Val(prix_produit)
         nNouvelleQuantite	est un entier	= Val(EDT_Nouvelle_Quantite)
         sStatutPaiement		est une chaîne	= COMBO_etat_paiement..ValeurAffichée
         sTypePaiement		est une chaîne	= COMBO_Type_Paiement..ValeurAffichée
         rMontantPaiement	est un réel		= Val(montant_paiement)
         dDateCheque			est une Date	= SAI_Date
         
         // Validation des champs
         SI nSaleID = 0 OU nClientID = 0 OU nArticleID = 0 OU nStockID = 0 ALORS
         	Erreur("Tous les champs obligatoires doivent être remplis.")
         	RETOUR
         FIN
         
         SI nQuantiteVendue <= 0 ALORS
         	Erreur("La quantité vendue doit être supérieure à zéro.")
         	RETOUR
         FIN
         
         SI nNouvelleQuantite < 0 ALORS
         	Erreur("La quantité réelle en stock ne peut pas être négative.")
         	RETOUR
         FIN
         
         // Confirmation utilisateur
         SI OuiNon("Confirmer la modification de la vente ?") = Faux ALORS
         	RETOUR
         FIN
         
         // Démarrage de la transaction
         HTransactionDébut()
         
         // 1. Mise à jour de la ligne de vente (sales_items)
         REQ_update_sales_items.paramsales_id				= nSaleID
         REQ_update_sales_items.paramitem_id					= nArticleID
         REQ_update_sales_items.paramitem_location_id		= nStockID
         REQ_update_sales_items.paramquantity_purchased		= nQuantiteVendue
         REQ_update_sales_items.paramunit_price				= rPrixUnitaire
         
         SI PAS HExécuteRequête(REQ_update_sales_items) ALORS
         	HTransactionAnnule()
         	Erreur("Erreur lors de la mise à jour des articles vendus : " + HErreurInfo())
         	RETOUR
         FIN
         
         // 2. Mise à jour de la vente (sales)
         REQ_update_sales.paramid				= nSaleID
         REQ_update_sales.paramcustomer_id		= nClientID
         REQ_update_sales.parampayment_status	= sStatutPaiement
         
         SI PAS HExécuteRequête(REQ_update_sales) ALORS
         	HTransactionAnnule()
         	Erreur("Erreur lors de la mise à jour de la vente : " + HErreurInfo())
         	RETOUR
         FIN
         
         // 3. Mise à jour du paiement (sales_payment)
         REQ_update_sales_payment.paramsale_id			= nSaleID
         REQ_update_sales_payment.parampayment_type		= sTypePaiement
         REQ_update_sales_payment.parampayment_amount	= rMontantPaiement
         REQ_update_sales_payment.paramdate_cheque		= dDateCheque
         
         SI PAS HExécuteRequête(REQ_update_sales_payment) ALORS
         	HTransactionAnnule()
         	Erreur("Erreur lors de la mise à jour du paiement : " + HErreurInfo())
         	RETOUR
         FIN
         
         // 4. Mise à jour du stock (item_quantities)
         REQ_update_quantities.Paramitem_id		= nArticleID
         REQ_update_quantities.Paramlocation_id	= nStockID
         REQ_update_quantities.Paramquantity		= nNouvelleQuantite
         
         SI PAS HExécuteRequête(REQ_update_quantities) ALORS
         	HTransactionAnnule()
         	Erreur("Erreur lors de la mise à jour de la quantité en stock : " + HErreurInfo())
         	RETOUR
         FIN
         
         // 5. Mise à jour du statut de la vente (sales_status)
         REQ_update_sales_status.Paramid				= nSaleID
         REQ_update_sales_status.Paramsales_status	= sStatutPaiement
         
         SI PAS HExécuteRequête(REQ_update_sales_status) ALORS
         	HTransactionAnnule()
         	Erreur("Erreur lors de la mise à jour du statut de la vente : " + HErreurInfo())
         	RETOUR
         FIN
         
         // Validation finale
         HTransactionFin()
         Info("Vente modifiée avec succès.")
         Ferme()
        type : 18
      -
        type : 161
        enabled : false
