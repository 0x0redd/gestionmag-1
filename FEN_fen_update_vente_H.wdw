 code_elements :
  type_code : 1
  p_codes :
   -
     code : |1-
      PROCÉDURE MaFenêtre(SaleId, location_id, gArticles_id)
      
      // Variables locales
      gnTotal_achat_cal			est un réel		= 0
      bRequetePrincipaleExecutee	est un booléen	= Faux
      nIndice						est un entier
      
      // 1. Récupération des données de la vente
      REQ_selection_vente_modif.paramid = SaleId
      SI HExécuteRequête(REQ_selection_vente_modif) ALORS
      	SI HLitPremier(REQ_selection_vente_modif) ALORS
      		// Assignation des valeurs aux champs
      		COMBO_client		= REQ_selection_vente_modif.person_id
      		COMBO_etat_paiement	= REQ_selection_vente_modif.sale_status
      		montant_paiement	= REQ_selection_vente_modif.payment_amount
      		SAI_Date			= REQ_selection_vente_modif.date_cheque
      		
      		// Vérification et assignation conditionnelle des valeurs
      		SI REQ_selection_vente_modif.item_location_id <> Null ET REQ_selection_vente_modif.item_location_id <> "" ALORS
      			COMBO_stock = REQ_selection_vente_modif.item_location_id
      		FIN
      		
      		SI REQ_selection_vente_modif.item_id <> Null ET REQ_selection_vente_modif.item_id <> "" ALORS
      			COMBO_article = REQ_selection_vente_modif.item_id
      		FIN
      		
      		SI REQ_selection_vente_modif.quantity_purchased <> Null ET REQ_selection_vente_modif.quantity_purchased <> "" ALORS
      			Quantitée = REQ_selection_vente_modif.quantity_purchased
      		FIN
      		
      		SI REQ_selection_vente_modif.item_unit_price <> Null ET REQ_selection_vente_modif.item_unit_price <> "" ALORS
      			prix_produit = REQ_selection_vente_modif.item_unit_price
      		FIN
      		
      		// Recherche dans la combo Type_Paiement par libellé
      		SI REQ_selection_vente_modif.payment_type <> Null ET REQ_selection_vente_modif.payment_type <> "" ALORS
      			nIndice = ListeCherche(COMBO_Type_Paiement, REQ_selection_vente_modif.payment_type)
      			SI nIndice > 0 ALORS
      				COMBO_Type_Paiement = nIndice
      			SINON
      				COMBO_Type_Paiement = 1 // Valeur par défaut
      			FIN
      		FIN
      		
      		bRequetePrincipaleExecutee = Vrai
      	SINON
      		Info("Aucun enregistrement trouvé pour l'ID de vente : " + SaleId)
      		Ferme()
      		RETOUR
      	FIN
      SINON
      	Erreur("Erreur lors de l'exécution de REQ_selection_vente_modif : " + HErreurInfo())
      	Ferme()
      	RETOUR
      FIN
      
      // 2. Récupération du stock si manquant
      SI COMBO_stock = "" OU COMBO_stock = 0 ALORS
      	REQ_select_stocklocation_modification.Paramid = location_id
      	SI HExécuteRequête(REQ_select_stocklocation_modification) ALORS
      		SI HLitPremier(REQ_select_stocklocation_modification) ALORS
      			COMBO_stock = REQ_select_stocklocation_modification.id
      		SINON
      			Info("Stock introuvable pour l'emplacement ID : " + location_id)
      		FIN
      	SINON
      		Erreur("Erreur lors de l'exécution de REQ_select_stocklocation_modification : " + HErreurInfo())
      		RETOUR
      	FIN
      FIN
      
      // 3. Récupération article/quantité si manquants
      SI COMBO_article = "" OU COMBO_article = 0 OU Quantitée = "" OU Quantitée = 0 ALORS
      	REQ_selection_salesItems.Paramsale_id = SaleId
      	SI HExécuteRequête(REQ_selection_salesItems) ALORS
      		SI HLitPremier(REQ_selection_salesItems) ALORS
      			SI COMBO_article = "" OU COMBO_article = 0 ALORS
      				COMBO_article = REQ_selection_salesItems.item_id
      			FIN
      			SI Quantitée = "" OU Quantitée = 0 ALORS
      				Quantitée = REQ_selection_salesItems.quantity_purchased
      			FIN
      			SI prix_produit = "" OU prix_produit = 0 ALORS
      				prix_produit = REQ_selection_salesItems.item_unit_price
      			FIN
      		SINON
      			Info("Aucun article trouvé dans la vente ID : " + SaleId)
      		FIN
      	SINON
      		Erreur("Erreur lors de l'exécution de REQ_selection_salesItems : " + HErreurInfo())
      		RETOUR
      	FIN
      FIN
      
      // 4. Affichage de la quantité disponible en stock
      REQ_total_quantity.param_item_id		= gArticles_id
      REQ_total_quantity.patam_location_id	= location_id // Correction de la faute de frappe
      SI HExécuteRequête(REQ_total_quantity) ALORS
      	SI HLitPremier(REQ_total_quantity) ALORS
      		STC_Quantite_Stock = REQ_total_quantity.quantity
      	SINON
      		STC_Quantite_Stock = 0
      		Info("Aucune quantité en stock trouvée pour l'article ID : " + gArticles_id)
      	FIN
      SINON
      	Erreur("Erreur lors du chargement de la quantité réelle : " + HErreurInfo())
      	RETOUR
      FIN
      
      // 5. Calcul du total de l'achat
      SI EstNumérique(Quantitée) ET EstNumérique(prix_produit) ALORS
      	gnTotal_achat_cal	= Val(Quantitée) * Val(prix_produit)
      	rTotal_achat est un réel			= gnTotal_achat_cal
      SINON
      	gnTotal_achat_cal	= 0
      	rTotal_achat			= 0
      	Info("Attention : Quantité ou prix non numérique. Total mis à 0.")
      FIN
      
      // Optionnel : Affichage du total calculé dans un champ
      SI gnTotal_achat_cal > 0 ALORS
      	// Supposons qu'il y ait un champ pour afficher le total
      	// total_achat_champ = NumériqueVersChaîne(gnTotal_achat_cal, "0.00")
      FIN
