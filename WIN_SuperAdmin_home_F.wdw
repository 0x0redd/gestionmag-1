   -
     name : PROCÉDURE
     procedure_id : 1748294827591689377
     type_code : 14
     code : |1+
      PROCÉDURE ChargerUtilisateurs()
      
      // Vider la table
      TableSupprimeTout(TABLE_Utilisateurs)
      
      // Parcourir les utilisateurs
      SI HLitPremier(users) ALORS
      	TANTQUE PAS HEnDehors(users)
      		
      		// Rechercher les infos liées
      		SI HLitRecherche(people, id, users.person_id) ALORS
      			SI HLitRecherche(role_user, user_id, users.id) ALORS
      				SI HLitRecherche(roles, id, role_user.role_id) ALORS
      					
      					// Ajouter l'utilisateur à la table mémoire
      					TableAjouteLigne(TABLE_Utilisateurs,
      					users.id, // ⚠️ Cette colonne doit exister dans la table mémoire (COL_ID)
      					people.first_name + " " + people.last_name,
      					users.email,
      					people.phone_number,
      					people.city,
      					people.country,
      					roles.name)
      				FIN
      			FIN
      		FIN
      		
      		HLitSuivant(users)
      	FIN
      FIN
      
      
     type : 458752
