  -
    name : BTN_AjouterPermission
    identifier : 0x2825f08113ee8db9
    internal_properties : 
    properties :
     x : 534
     y : 19
     width : 136
     height : 101
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         // Version 1
         // Description 
         // Bouton au style Nouveau pour ajouter ou créer des éléments
         // Vérifier qu'une ligne est sélectionnée dans la table des permissions disponibles
        type : 14
      -
        code : |1+
         // Vérifier la sélection
         SI TableSelect(TABLE_PermissionsDispo) = -1 ALORS
         	Info("Veuillez sélectionner une permission à ajouter.")
         	RETOUR
         FIN
         
         SI COMBO_Role = "" ALORS
         	Info("Veuillez sélectionner un rôle.")
         	RETOUR
         FIN
         
         // Extraire les IDs
         idRole			est un entier	= Val(ExtraitChaîne(COMBO_Role, 2, TAB))
         idPermission	est un entier	= TABLE_PermissionsDispo[TableSelect(TABLE_PermissionsDispo)].COL_id
         
         // Paramétrer la requête
         REQ_ajouter_permission.Paramrole_id			= idRole
         REQ_ajouter_permission.Parampermission_id	= idPermission
         
         SI HExécuteRequête(REQ_ajouter_permission) = Faux ALORS
         	Info("Erreur lors de l'ajout : " + HErreurInfo())
         	RETOUR
         FIN
         
         // Recharger les listes
         idDernierRole = 0 // Force la recharge même si même rôle
         ExecuteProcess(COMBO_Role, trtSélection)
