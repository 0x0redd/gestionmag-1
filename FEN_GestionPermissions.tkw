  -
    name : COMBO_Role
    identifier : 0x2825ea9911be7b79
    internal_properties : 
    properties :
     x : 49
     y : 29
     width : 442
     height : 81
    dropdown :
     internal_properties : 
     type : 7
    type : 14
    code_elements :
     type_code : 23
     p_codes :
      -
        code : |1+
         // Version 1
         // Description
         // Combo liste simple
         
         MaRequeteSQL	est une chaîne	= "SELECT id, name FROM roles"
         SourceRoles		est un Data Source
         
         SI HExécuteRequêteSQL(SourceRoles, Connexion_db, MaRequeteSQL) = Faux ALORS
         	Info("Erreur de requête : " + HErreurInfo())
         	RETOUR
         FIN
         
         // Nettoyer la combo
         ListeSupprimeTout(COMBO_Role)
         
         // Ajouter les éléments
         TANTQUE HLitSuivant(SourceRoles)
         	ListeAjoute(COMBO_Role, SourceRoles.name + TAB + SourceRoles.id)
         FIN
         
         // Sélection automatique du 1er élément
         SI ListeOccurrence(COMBO_Role) > 0 ALORS
         	COMBO_Role..Valeur = COMBO_Role[1]
         FIN
         
        type : 14
      -
        type : 35
        enabled : false
      -
        code : |1+
         SI COMBO_Role = "" ALORS
         	RETOUR
         FIN
         
         // Extraire l'ID sélectionné
         idRole est un entier = Val(ExtraitChaîne(COMBO_Role, 2, TAB))
         
         // Empêcher les recharges inutiles
         SI idRole = idDernierRole ALORS
         	// Si même rôle, ne pas recharger
         	RETOUR
         FIN
         
         // Mettre à jour le dernier rôle sélectionné
         idDernierRole						= idRole
         
         // → Requête des permissions attribuées
         REQ_permissions_attrib.Paramrole_id	= idRole
         
         SI HExécuteRequête(REQ_permissions_attrib) = Faux ALORS
         	Info("Erreur chargement permissions attribuées : " + HErreurInfo())
         	RETOUR
         FIN
         
         TableSupprimeTout(TABLE_PermissionsAttrib)
         
         POUR TOUT REQ_permissions_attrib
         	TableAjouteLigne(TABLE_PermissionsAttrib, REQ_permissions_attrib.id, REQ_permissions_attrib.name)
         FIN
         
         // → Requête des permissions disponibles
         REQ_permissions_dispo.Paramrole_id = idRole
         
         SI HExécuteRequête(REQ_permissions_dispo) = Faux ALORS
         	Info("Erreur chargement permissions disponibles : " + HErreurInfo())
         	RETOUR
         FIN
         
         TableSupprimeTout(TABLE_PermissionsDispo)
         
         POUR TOUT REQ_permissions_dispo
         	TableAjouteLigne(TABLE_PermissionsDispo, REQ_permissions_dispo.id, REQ_permissions_dispo.name)
         FIN
         
         
         
        type : 33
      -
