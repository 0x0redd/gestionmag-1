  p_codes :
   -
     code : |1+
      // Vider la zone répétée
      TableSupprimeTout(ZR_StockCritique)
      
      // Exécuter la requête
      SI PAS HExécuteRequête(REQ_select_articles) ALORS
      	Erreur("Erreur d'exécution de la requête")
      	RETOUR
      FIN
      
      // Parcourir les résultats
      HLitPremier(REQ_select_articles)
      TANTQUE PAS HEnDehors()
      	// Filtre : uniquement les produits en seuil critique
      	SI REQ_select_articles.quantity <= REQ_select_articles.seuil_critique ALORS
      		// Ajouter une ligne vide dans la zone répétée
      		nIndice est un entier = TableAjouteLigne(ZR_StockCritique)
      		
      		// Remplir les libellés de la ligne ajoutée
      		ZR_StockCritique[nIndice].LIB_Article	= REQ_select_articles.name
      		ZR_StockCritique[nIndice].LIB_qte		= REQ_select_articles.quantity
      	FIN
      	
      	HLitSuivant(REQ_select_articles)
      FIN
