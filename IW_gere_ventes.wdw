            // --- √âtape 1 : R√©cup√©rer l'ID de la vente (sale_id) ---
            SI PAS HEx√©cuteRequ√™te(REQ_select_max_id_sales) ALORS
            	Erreur("Impossible de r√©cup√©rer l'ID de la vente : " + HErreurInfo())
            	RETOUR
            FIN
            
            SI HLitPremier(REQ_select_max_id_sales) = Faux ALORS
            	Erreur("Aucune vente trouv√©e.")
            	RETOUR
            FIN
            
            sale_id		est un entier	= REQ_select_max_id_sales.le_maximum_id
            
            // --- √âtape 2 : Initialiser le total saisi ---
            nTotalSaisi	est un r√©el		= 0
            
            // --- √âtape 3 : Paiement par ch√®que ---
            SI cheque_saisie <> "" ALORS
            	REQ_sales_payments.Parampayment_type	= "Ch√®que"
            	REQ_sales_payments.Parampayment_amount	= Val(cheque_saisie)
            	REQ_sales_payments.Paramsale_id			= sale_id
            	REQ_sales_payments.Paramdate_cheque		= SAI_Date_cheque
            	REQ_sales_payments.Paramcreated_at		= DateSys()
            
            	SI PAS HEx√©cuteRequ√™te(REQ_sales_payments) ALORS
            		Erreur("Erreur lors du paiement par ch√®que : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	nTotalSaisi += Val(cheque_saisie)
            FIN
            
            // --- √âtape 4 : Paiement en esp√®ce ---
            SI espece_saisie <> "" ALORS
            	REQ_sales_payments.Parampayment_type	= "Esp√®ce"
            	REQ_sales_payments.Parampayment_amount	= Val(espece_saisie)
            	REQ_sales_payments.Paramsale_id			= sale_id
            	REQ_sales_payments.Paramdate_cheque		= Null
            	REQ_sales_payments.Paramcreated_at		= DateSys()
            
            	SI PAS HEx√©cuteRequ√™te(REQ_sales_payments) ALORS
            		Erreur("Erreur lors du paiement en esp√®ce : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	nTotalSaisi += Val(espece_saisie)
            FIN
            
            // --- √âtape 5 : Paiement par carte ---
            SI carte_saisie <> "" ALORS
            	REQ_sales_payments.Parampayment_type	= "Carte"
            	REQ_sales_payments.Parampayment_amount	= Val(carte_saisie)
            	REQ_sales_payments.Paramsale_id			= sale_id
            	REQ_sales_payments.Paramdate_cheque		= Null
            	REQ_sales_payments.Paramcreated_at		= DateSys()
            
            	SI PAS HEx√©cuteRequ√™te(REQ_sales_payments) ALORS
            		Erreur("Erreur lors du paiement par carte : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	nTotalSaisi += Val(carte_saisie)
            FIN
            
            // --- √âtape 6 : V√©rification du paiement complet ou non ---
            nMontantTotal est un r√©el = Val(LIB_total_achat) // √† adapter selon le champ
            
            SI nTotalSaisi >= nMontantTotal ALORS
            	// Paiement complet
            	REQ_update_sales_status.Paramid				= sale_id
            	REQ_update_sales_status.Paramsales_status	= "pay√©e"
            
            	SI PAS HEx√©cuteRequ√™te(REQ_update_sales_status) ALORS
            		Erreur("Erreur lors de la mise √† jour du statut de vente : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	Info("Vente enregistr√©e avec paiement complet.")
            SINON
            	// Paiement partiel
            	nMontantRestant est un r√©el = nMontantTotal - nTotalSaisi
            
            	REQ_update_sales_status.Paramid				= sale_id
