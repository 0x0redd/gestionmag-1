            	// Requête pour récupérer la quantité en stock depuis la table item_quantities
            	REQ_get_stock.ParamItemID		= idArticle
            	REQ_get_stock.ParamLocationID	= location_id
            
            	SI HExécuteRequête(REQ_get_stock) ALORS
            		SI HLitPremier(REQ_get_stock) ALORS
            			SAI_QteTotal = REQ_get_stock.quantity
            		SINON
            			SAI_QteTotal = 0
            		FIN
            	SINON
            Erreur("Erreur lors de la lecture du stock : " + HErreurInfo())
            FIN
            FIN
