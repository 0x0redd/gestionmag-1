       name : BTN_Valider
       identifier : 0x2848cfec2e6ad46b
       internal_properties : 
       properties :
        x : 676
        y : 790
        width : 109
        height : 32
        accelerator_key : 65535
        tab_order : 16
       style : {}
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1-
            
           type : 14
         -
           code : |1-
            // 1. Récupérer les valeurs des combos
            nItemID		est un entier	= COMBO_article1
            nLocationID	est un entier	= COMBO_stock1
            nCustomerID	est un entier	= COMBO_clients1
            
            // 2. Vérification des champs obligatoires
            SI nItemID = 0 ALORS
            	Erreur("Veuillez sélectionner un article.")
            	RETOUR
            FIN
            SI nLocationID = 0 ALORS
            	Erreur("Veuillez sélectionner un stock.")
            	RETOUR
            FIN
            SI nCustomerID = 0 ALORS
            	Erreur("Veuillez sélectionner un client.")
            	RETOUR
            FIN
            
            // 3. Récupération de la quantité et validation
            nQuantite est un entier = Val(Quantitée)
            SI nQuantite <= 0 ALORS
            	Erreur("Veuillez saisir une quantité valide.")
            	RETOUR
            FIN
            
            // 4. Vérification du stock disponible
            SI nQuantite > Val(SAI_QteTotal) ALORS
            	Erreur("Stock insuffisant. Quantité disponible: " + SAI_QteTotal)
            	RETOUR
            FIN
            
            // 5. Lire les infos de l'article pour récupérer les prix
            REQ_select_prices.Paramid = nItemID
            SI PAS HExécuteRequête(REQ_select_prices) ALORS
            	Erreur("Impossible de récupérer les prix de l'article : " + HErreurInfo())
            	RETOUR
            FIN
            
            SI HLitPremier(REQ_select_prices) = Faux ALORS
            	Erreur("Article introuvable.")
            	RETOUR
            FIN
            
            nCostPrice		est un réel		= REQ_select_prices.cost_price
            nUnitPrice		est un réel		= REQ_select_prices.unit_price
            
            // 6. Préparation des données communes
            sCommentaire	est une chaîne	= EDT_Commentaire
            sInvoiceNumber	est une chaîne	= "FACT-" + NumériqueVersChaîne(HeureSys(), "0")
            sDate			est une chaîne	= DateVersChaîne(DateSys(), "AAAA-MM-JJ HH:MM:SS")
            nUserID			est un entier	= 1	// à adapter selon votre système
            
            reference1										= sInvoiceNumber
            
            // 7. Insertion de la vente principale
            REQ_ajout_sale_insert.Paramcustomer_id			= nCustomerID
            REQ_ajout_sale_insert.Paramcommentaire			= sCommentaire
            REQ_ajout_sale_insert.Paraminvoice_number		= sInvoiceNumber
            REQ_ajout_sale_insert.Paramsale_status			= "en cours"
            REQ_ajout_sale_insert.Paramlocation_id			= nLocationID
            REQ_ajout_sale_insert.ParamsalePayment_status	= "non payé"
            REQ_ajout_sale_insert.Paramcreated_at			= sDate
            REQ_ajout_sale_insert.Paramuser_id				= nUserID
            
            SI PAS HExécuteRequête(REQ_ajout_sale_insert) ALORS
            	Erreur("Erreur lors de l'ajout de la vente : " + HErreurInfo())
            	RETOUR
            FIN
            
            // 8. Récupération de l'ID de la vente créée
            SI PAS HExécuteRequête(REQ_select_max_id_sales) ALORS
            	Erreur("Impossible de récupérer l'ID de la vente : " + HErreurInfo())
            	RETOUR
            FIN
            
            SI HLitPremier(REQ_select_max_id_sales) = Faux ALORS
            	Erreur("Aucune vente trouvée.")
            	RETOUR
            FIN
            
            nSaleID est un entier = REQ_select_max_id_sales.le_maximum_id
            
            // 9. Insertion dans sales_items
            REQ_Ajout_sales_items.Paramsale_id				= nSaleID
            REQ_Ajout_sales_items.Paramitem_id				= nItemID
            REQ_Ajout_sales_items.Paramdescription			= ""  // Ajout de valeur par défaut
            REQ_Ajout_sales_items.Paramserialnumber			= ""
            REQ_Ajout_sales_items.Paramquantity_purchased	= nQuantite
            REQ_Ajout_sales_items.Paramitem_cost_price		= nCostPrice
            REQ_Ajout_sales_items.Paramitem_unit_price		= nUnitPrice
            REQ_Ajout_sales_items.Paramitem_location		= nLocationID
            REQ_Ajout_sales_items.Paramcreated_at			= sDate
            
            SI PAS HExécuteRequête(REQ_Ajout_sales_items) ALORS
            	Erreur("Erreur lors de l'ajout des articles : " + HErreurInfo())
            	RETOUR
            FIN
            
            // 10. Calcul du total pour information
            LIB_total_achat								= nQuantite * nUnitPrice
            
            // 11. Mise à jour du stock - Vérifier l'existence de l'article dans le stock
            REQ_Verifier_Item_Stock.Paramitem_id		= nItemID
            REQ_Verifier_Item_Stock.Paramlocation_id	= nLocationID
            
            SI HExécuteRequête(REQ_Verifier_Item_Stock) = Faux ALORS
            	Erreur("Erreur lors de la vérification du stock : " + HErreurInfo())
            	RETOUR
            FIN
            
            SI HNbEnr(REQ_Verifier_Item_Stock) > 0 ALORS
            	// L'article existe dans le stock, on met à jour la quantité
            	SI HLitPremier(REQ_Verifier_Item_Stock) = Faux ALORS
            		Erreur("Erreur lors de la lecture du stock existant.")
            		RETOUR
            	FIN
            
            	idQuantiteStock		est un entier	= REQ_Verifier_Item_Stock.id
            
            	// Calcul de la nouvelle quantité (soustraction pour la vente)
            	nNouvelleQuantite	est un entier	= Val(SAI_QteTotal) - nQuantite
            	
            	// Mise à jour de la quantité dans le stock
            	REQ_modifier_items_quantities.Paramitem_id		= idQuantiteStock
            	REQ_modifier_items_quantities.Paramquantity	= nNouvelleQuantite
            
            	SI PAS HExécuteRequête(REQ_modifier_items_quantities) ALORS
            		Erreur("Erreur lors de la mise à jour du stock : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	// Mise à jour de l'affichage
            	SAI_QteTotal = nNouvelleQuantite
            
            SINON
            	Erreur("Article non trouvé dans le stock spécifié.")
            	RETOUR
            FIN
            
            Info("Vente et mise à jour du stock effectuées avec succès.")
