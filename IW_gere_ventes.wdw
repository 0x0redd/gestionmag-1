           code : |1+
            // Récupérer l'ID du produit et de l'emplacement sélectionnés
            idArticle	est un entier	= COMBO_article1..Valeur
            location_id	est un entier	= COMBO_stock1..Valeur
            
            // Vérifier que l'article a bien été sélectionné
            SI idArticle <> 0 ALORS
            
            	// Lire les informations de l'article dans la table "items"
            	SI HLitRecherche(items, id, idArticle) ALORS
            
            		// Affecter les valeurs dans les champs de l'IHM
            		Produit_name	= items.name
            		prix_produit	= items.cost_price
            
            		// Générer une référence unique de vente (ex: 20250627-0045)
            		sDate		est une chaîne				= DateVersChaîne(DateSys(), "AAAAMMJJ")
            		sIDProduit	est une chaîne				= NumériqueVersChaîne(idArticle, "04")
