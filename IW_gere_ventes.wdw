       name : BTN_SansNom3
       identifier : 0x2848cfec2e6cd46b
       internal_properties : 
       properties :
        x : 884
        y : 791
        width : 161
        height : 57
        anchor : 5
        anchor_ratio_pos_y : 500
        accelerator_key : 65535
        tab_order : 17
       style : {}
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1+
            // --- Étape 1 : Récupérer l'ID de la vente (sale_id) ---
            SI PAS HExécuteRequête(REQ_select_max_id_sales) ALORS
            	Erreur("Impossible de récupérer l'ID de la vente : " + HErreurInfo())
            	RETOUR
            FIN
            
            SI HLitPremier(REQ_select_max_id_sales) = Faux ALORS
            	Erreur("Aucune vente trouvée.")
            	RETOUR
            FIN
            
            sale_id		est un entier	= REQ_select_max_id_sales.le_maximum_id
            
            // --- Étape 2 : Initialiser le total saisi ---
            nTotalSaisi	est un réel		= 0
            
            // --- Étape 3 : Paiement par chèque ---
            SI cheque_saisie <> "" ALORS
            	REQ_sales_payments.Parampayment_type	= "Chèque"
            	REQ_sales_payments.Parampayment_amount	= Val(cheque_saisie)
            	REQ_sales_payments.Paramsale_id			= sale_id
            	REQ_sales_payments.Paramdate_cheque		= SAI_Date_cheque
            	REQ_sales_payments.Paramcreated_at		= DateSys()
            
            	SI PAS HExécuteRequête(REQ_sales_payments) ALORS
            		Erreur("Erreur lors du paiement par chèque : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	nTotalSaisi += Val(cheque_saisie)
            FIN
            
            // --- Étape 4 : Paiement en espèce ---
            SI espece_saisie <> "" ALORS
            	REQ_sales_payments.Parampayment_type	= "Espèce"
            	REQ_sales_payments.Parampayment_amount	= Val(espece_saisie)
            	REQ_sales_payments.Paramsale_id			= sale_id
            	REQ_sales_payments.Paramdate_cheque		= Null
            	REQ_sales_payments.Paramcreated_at		= DateSys()
            
            	SI PAS HExécuteRequête(REQ_sales_payments) ALORS
            		Erreur("Erreur lors du paiement en espèce : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	nTotalSaisi += Val(espece_saisie)
            FIN
            
            // --- Étape 5 : Paiement par carte ---
            SI carte_saisie <> "" ALORS
            	REQ_sales_payments.Parampayment_type	= "Carte"
            	REQ_sales_payments.Parampayment_amount	= Val(carte_saisie)
            	REQ_sales_payments.Paramsale_id			= sale_id
            	REQ_sales_payments.Paramdate_cheque		= Null
            	REQ_sales_payments.Paramcreated_at		= DateSys()
            
            	SI PAS HExécuteRequête(REQ_sales_payments) ALORS
            		Erreur("Erreur lors du paiement par carte : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	nTotalSaisi += Val(carte_saisie)
            FIN
            
            // --- Étape 6 : Vérification du paiement complet ou non ---
            nMontantTotal est un réel = Val(LIB_total_achat) // à adapter selon le champ
            
            SI nTotalSaisi >= nMontantTotal ALORS
            	// Paiement complet
            	REQ_update_sales_status.Paramid				= sale_id
            	REQ_update_sales_status.Paramsales_status	= "payée"
            
            	SI PAS HExécuteRequête(REQ_update_sales_status) ALORS
            		Erreur("Erreur lors de la mise à jour du statut de vente : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	Info("Vente enregistrée avec paiement complet.")
            SINON
            	// Paiement partiel
            	nMontantRestant est un réel = nMontantTotal - nTotalSaisi
            
            	REQ_update_sales_status.Paramid				= sale_id
            	REQ_update_sales_status.Paramsales_status	= "non complet"
            
            	SI PAS HExécuteRequête(REQ_update_sales_status) ALORS
            		Erreur("Erreur mise à jour statut vente : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	Info("Paiement partiel effectué. Reste à payer : " + nMontantRestant + " DA")
            FIN
