            	SI PAS HEx√©cuteRequ√™te(REQ_update_sales_status) ALORS
            		Erreur("Erreur mise √† jour statut vente : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	Info("Paiement partiel effectu√©. Reste √† payer : " + nMontantRestant + " DA")
            FIN
