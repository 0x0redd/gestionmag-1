           code : |1+
             nItemID			est un entier	= COMBO_article1
            nLocationID		est un entier	= COMBO_stock1
            nCustomerID		est un entier	= COMBO_clients1
            
            // V√©rification des champs
            SI nItemID = 0 OU nLocationID = 0 OU nCustomerID = 0 ALORS
            	Erreur("Veuillez s√©lectionner un article, un stock et un client.")
            	RETOUR
            FIN
            
            SI Val(Quantit√©e) > Val(SAI_QteTotal) ALORS
            	INFO(Quantit√©e , SAI_QteTotal)
            	Erreur("le stock est insufissant")
            	RETOUR
            FIN
            
            
            
            // R√©cup√©ration de la quantit√©
            nQuantite est un entier = Val(Quantit√©e)
            SI nQuantite <= 0 ALORS
            	Erreur("Veuillez saisir une quantit√© valide.")
            	RETOUR
            FIN
            
            // Pr√©paration des donn√©es
            sCommentaire		est une cha√Æne	= EDT_Commentaire
            sInvoiceNumber		est une cha√Æne	= "FACT-" + Num√©riqueVersCha√Æne(HeureSys(), "0")
            sDate				est une cha√Æne	= DateVersCha√Æne(DateSys(), "AAAA-MM-JJ HH:MM:SS")
            nUserID				est un entier	= 1	// √† adapter
            
            reference1 = sInvoiceNumber
            
            // Insertion de la vente (une seule fois !)
            REQ_ajout_sale_insert.Paramcustomer_id			= nCustomerID
            REQ_ajout_sale_insert.Paramcommentaire			= sCommentaire
            REQ_ajout_sale_insert.Paraminvoice_number		= sInvoiceNumber
            REQ_ajout_sale_insert.Paramsale_status			= "en cours"
            REQ_ajout_sale_insert.Paramlocation_id			= nLocationID
            REQ_ajout_sale_insert.ParamsalePayment_status	= "non pay√©"
            REQ_ajout_sale_insert.Paramcreated_at			= sDate
            REQ_ajout_sale_insert.Paramuser_id				= nUserID
            
            SI PAS HEx√©cuteRequ√™te(REQ_ajout_sale_insert) ALORS
            	Erreur("Erreur lors de l'ajout de la vente : " + HErreurInfo())
            	RETOUR
            FIN
            
            // R√©cup√©ration de l'ID de la vente cr√©√©e
            SI PAS HEx√©cuteRequ√™te(REQ_select_max_id_sales) ALORS
            	Erreur("Impossible de r√©cup√©rer l'ID de la vente : " + HErreurInfo())
            	RETOUR
            FIN
            
            SI HLitPremier(REQ_select_max_id_sales) = Faux ALORS
            	Erreur("Aucune vente trouv√©e.")
            	RETOUR
            FIN
            
            nSaleID est un entier = REQ_select_max_id_sales.le_maximum_id
            
            // R√©cup√©ration des prix
            REQ_select_prices.Paramid = nItemID
            SI PAS HEx√©cuteRequ√™te(REQ_select_prices) ALORS
            	Erreur("Impossible de r√©cup√©rer les prix de l'article : " + HErreurInfo())
            	RETOUR
            FIN
            
            HLitPremier(REQ_select_prices)
            nCostPrice	est un r√©el	= REQ_select_prices.cost_price
            nUnitPrice	est un r√©el	= REQ_select_prices.unit_price
            
            // Insertion dans sales_items
            REQ_Ajout_sales_items.Paramsale_id				= nSaleID
            REQ_Ajout_sales_items.Paramitem_id				= nItemID
            REQ_Ajout_sales_items.Paramdescription			= "Article vendu"
            REQ_Ajout_sales_items.Paramserialnumber			= ""
            REQ_Ajout_sales_items.Paramquantity_purchased	= nQuantite
            REQ_Ajout_sales_items.Paramitem_cost_price		= nCostPrice
            REQ_Ajout_sales_items.Paramitem_unit_price		= nUnitPrice
            REQ_Ajout_sales_items.Paramitem_location		= nLocationID
            REQ_Ajout_sales_items.Paramcreated_at			= sDate
            
            SI PAS HEx√©cuteRequ√™te(REQ_Ajout_sales_items) ALORS
            	Erreur("Erreur lors de l'ajout des articles : " + HErreurInfo())
            	RETOUR
            FIN
            LIB_total_achat = val(total_achat)
            
            // Affectation des param√®tres de la requ√™te
            REQ_Ajout_sales_items.Paramitem_id			= nItemID
            REQ_Ajout_sales_items.Paramitem_location	= nLocationID
            
            // Ex√©cution de la requ√™te
            SI PAS HEx√©cuteRequ√™te(REQ_select_items_quantites_md) ALORS
            	Erreur("Erreur lors de la r√©cup√©ration de la quantit√© : " + HErreurInfo())
            	RETOUR
            FIN
            
            // Lecture du premier r√©sultat
            SI HLitPremier(REQ_select_items_quantites_md) = Faux ALORS
            	Erreur("Article introuvable dans les stocks.")
            	RETOUR
            FIN
            
            // Lecture de la quantit√© actuelle
            nAncienneQuantite	est un entier	= REQ_select_items_quantites_md.quantity
            
            // Calcul de la nouvelle quantit√©
            nNouvelleQuantite	est un entier	= Val(SAI_QteTotal) - Val(Quantit√©e)
            
            
            
            // Mise √† jour
            
            REQ_modifier_items_quantities.Paramitem_id = nItemID
            REQ_modifier_items_quantities.Paramquantity		= nNouvelleQuantite
            
            SI PAS HEx√©cuteRequ√™te(REQ_modifier_items_quantities) ALORS
            	Erreur("Erreur lors de la mise √† jour des quantit√©s : " + HErreurInfo())
            	RETOUR
            FIN
            
            Info("Vente et mise √† jour du stock effectu√©es avec succ√®s.")
            SAI_QteTotal = nNouvelleQuantite
