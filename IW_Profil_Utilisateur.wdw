  procedures :
   -
     name : profilUtilisateur
     procedure_id : 1755960811672994539
     type_code : 14
     code : |1+
      PROCÉDURE profilUtilisateur()
      // Formater nom et prénom
      sNomMajuscule		est une chaîne	= Majuscule(gNomUtilisateur)
      sPrenomFormate		est une chaîne	= Majuscule(Milieu(gPrenomUtilisateur, 1, 1)) + Minuscule(Milieu(gPrenomUtilisateur, 2))
      
      // Affichage des informations générales
      LIB_Nom		= "Nom : " + sNomMajuscule
      LIB_Prenom	= "Prénom : " + sPrenomFormate
      LIB_Email	= "Email : " + gEmailUtilisateur
      LIB_Role	= "Rôle : " + gRoleUtilisateur
      
      // Si tu as d'autres infos à afficher depuis la table people
      SI HLitRecherche(people, id, users.person_id) ALORS
      	LIB_Telephone	= "Téléphone : " + people.phone_number
      	LIB_Adresse		= "Adresse : " + people.address_1
      	LIB_Ville		= "Ville : " + people.city
      	LIB_Pays		= "Pays : " + people.country
      //	LIB_DateNaiss	= "Date de naissance : " + DateVersChaîne(people.birth_date, "JJ/MM/AAAA")
          LIB_DateInscription = "Inscrit le : " + DateVersChaîne(users.created_at, "JJ/MM/AAAA")
      FIN
      
      // Tu peux aussi afficher la date d'inscription ou le statut si disponible
      // LIB_DateInscription = "Inscrit le : " + DateVersChaîne(users.created_at, "JJ/MM/AAAA")
      // LIB_Statut = "Statut : " + users.status
     type : 458752
