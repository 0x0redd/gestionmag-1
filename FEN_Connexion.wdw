     p_codes :
      -
        code : |1+
         // Version 1
         // Description 
         // Bouton en validation
         
        type : 14
      -
        code : |1+
         // Récupération des données saisies
         sEmail			est une chaîne	= EDT_Email
         sMotDePasse		est une chaîne	= EDT_MotDePasse
         
         // Recherche de l'utilisateur dans la base avec l'e-mail
         SI HLitRecherche(users, email, sEmail) = Faux ALORS
         	Info("Utilisateur introuvable.")
         	RETOUR
         FIN
         
         // Comparaison du mot de passe en clair (⚠️ temporaire)
         SI users.password <> sMotDePasse ALORS
         	Info("Mot de passe incorrect.")
         	RETOUR
         FIN
         
         // Récupération du rôle de l'utilisateur
         SI HLitRecherche(role_user, user_id, users.id) = Faux ALORS
         	Info("Aucun rôle attribué à cet utilisateur.")
         	RETOUR
         FIN
         
         SI HLitRecherche(roles, id, role_user.role_id) = Faux ALORS
         	Info("Rôle introuvable.")
         	RETOUR
         FIN
         
         // Récupération des informations du client lié
         SI HLitRecherche(people, id, users.person_id) = Faux ALORS
         	Info("Personne liée introuvable.")
         	RETOUR
         FIN
         
         // Variables globales
         gUtilisateurID		= users.id
         gNomUtilisateur		= people.last_name
         gPrenomUtilisateur	= people.first_name
         
         // Redirection selon l'ID du rôle
         SELON role_user.role_id
         	CAS 1
         		Ouvre(FEN_Home) //FEN_SuperAdmin
         	CAS 2
         		Ouvre(FEN_Home) //FEN_Responsable
         	CAS 3
         		Ouvre(FEN_Home) //FEN_Magasinier
         	AUTRE CAS
         		Info("Rôle ID non reconnu : " + role_user.role_id)
         FIN
         
         
         // Fermer la fenêtre de connexion
         Ferme()
