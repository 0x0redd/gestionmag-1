            // Bouton simple
            
           type : 14
         -
           code : |1-
            // 1. Récupérer les valeurs des combos
            nItemID		est un entier	= COMBO_Article
            nLocationID	est un entier	= COMBO_Stock
            nSupplierID	est un entier	= COMBO_Fournisseur
            
            SI nItemID = 0 ALORS
            	Erreur("Veuillez sélectionner un article.")
            	RETOUR
            FIN
            SI nLocationID = 0 ALORS
            	Erreur("Veuillez sélectionner un stock.")
            	RETOUR
            FIN
            SI nSupplierID = 0 ALORS
            	Erreur("Veuillez sélectionner un fournisseur.")
            	RETOUR
            FIN
            
            // 2. Lire les infos de l'article
            SI HLitRecherche(items, id, nItemID) = Faux ALORS
            	Erreur("Article introuvable.")
            	RETOUR
            FIN
            
            prix_produit	est un réel		= items.cost_price
            Produit_name	est une chaîne	= items.name
            
            // 3. Récupération des champs saisis - CORRECTION : Utiliser Val() pour la conversion
            nQuantite		est un entier	= Val(Quantitée)
            
            SI nQuantite <= 0 ALORS
            	Erreur("Veuillez saisir une quantité valide.")
            	RETOUR
            FIN
            
            sDate est une chaîne = DateVersChaîne(DateSys(), "AAAA-MM-JJ HH:MM:SS")
            
            // 4. Vérifier si l'article existe déjà dans le stock
            REQ_Verifier_Item_Stock.Paramitem_id		= nItemID
            REQ_Verifier_Item_Stock.Paramlocation_id	= nLocationID
            
            SI HExécuteRequête(REQ_Verifier_Item_Stock) = Faux ALORS
            	Erreur("Erreur lors de la vérification : " + HErreurInfo())
            	RETOUR
            FIN
            
            SI HNbEnr(REQ_Verifier_Item_Stock) > 0 ALORS
            	// L'article existe, on met à jour la quantité
            	// CORRECTION : Lire l'enregistrement avant d'accéder aux données
            	SI HLitPremier(REQ_Verifier_Item_Stock) = Faux ALORS
            		Erreur("Erreur lors de la lecture du stock existant.")
            		RETOUR
            	FIN
            
            	idQuantite est un entier = REQ_Verifier_Item_Stock.id
            
            	// Utiliser une requête avec "quantity = quantity + {Paramquantity}"
            	REQ_MAJ_Quantite.Paramid		= idQuantite
            	REQ_MAJ_Quantite.Paramquantity	= nQuantite
            
            	SI HExécuteRequête(REQ_MAJ_Quantite) = Faux ALORS
            		Erreur("Erreur lors de la mise à jour : " + HErreurInfo())
            		RETOUR
            	FIN
            
            SINON
            	// Sinon, on insère un nouveau enregistrement
            	REQ_Ajout_Items_Quantites.Paramitem_id		= nItemID
            	REQ_Ajout_Items_Quantites.Paramlocation_id	= nLocationID
            	REQ_Ajout_Items_Quantites.Paramquantity		= nQuantite
            	REQ_Ajout_Items_Quantites.Paramcreated_at	= sDate
            
            	SI HExécuteRequête(REQ_Ajout_Items_Quantites) = Faux ALORS
            		Erreur("Erreur lors de l'ajout : " + HErreurInfo())
            		RETOUR
            	FIN
            FIN
            
            // 5. Calcul du total (variable utilisée pour information)
            price			est un réel	= prix_produit
            total_achat_cal	est un réel	= nQuantite * price
            
            // 6. Insertion dans receivings
            REQ_Ajout_receivings.Paramreceiving_time			= DateSys()
            REQ_Ajout_receivings.Paramsupplier_id				= nSupplierID
            REQ_Ajout_receivings.Paramuser_id					= 1
            REQ_Ajout_receivings.Paramcomment					= EDT_Commentaire
            REQ_Ajout_receivings.Paramreference					= reference
            REQ_Ajout_receivings.Paramreceiving_type			= selecteur..Valeur
            REQ_Ajout_receivings.ParamreceivingPayment_status	= "non défini"
            REQ_Ajout_receivings.Paramcreated_at				= sDate
            
            // Exécuter une seule fois la requête
            SI HExécuteRequête(REQ_Ajout_receivings) = Faux ALORS
            	Erreur("Erreur lors de l'insertion de la réception : " + HErreurInfo())
            	RETOUR
            FIN
            
            // --- Récupérer l'ID du receiving à utiliser ---
            SI PAS HExécuteRequête(REQ_select_max_id_receivings) ALORS
            	Erreur("Erreur lors de l'exécution de la requête : " + HErreurInfo())
            	RETOUR
            FIN
            
            SI HLitPremier(REQ_select_max_id_receivings) = Faux ALORS
            	Erreur("Aucun ID de receiving trouvé.")
            	RETOUR
            FIN
            
            nReceiving_ID est un entier = REQ_select_max_id_receivings.le_maximum_id
            
            // --- Insérer dans receiving_items ---
            // CORRECTION : Utiliser la variable nQuantite (entier) au lieu de Quantitée (chaîne)
            REQ_Insert_Receiving_Item.pReceivingID			= nReceiving_ID
            REQ_Insert_Receiving_Item.pItemID				= nItemID
            REQ_Insert_Receiving_Item.pDescription			= Produit_name
            REQ_Insert_Receiving_Item.pQuantityPurchased	= nQuantite		// CORRECTION
            REQ_Insert_Receiving_Item.pItemCostPrice		= prix_produit
            REQ_Insert_Receiving_Item.pItemUnitPrice		= prix_produit
            REQ_Insert_Receiving_Item.pItemLocation			= nLocationID
            REQ_Insert_Receiving_Item.pReceivingQuantity	= nQuantite		// CORRECTION
            REQ_Insert_Receiving_Item.pCreatedAt			= DateSys()
            
            SI HExécuteRequête(REQ_Insert_Receiving_Item) = Faux ALORS
            	Erreur("Erreur lors de l'insertion dans receiving_items : " + HErreurInfo())
            	RETOUR
            SINON
            	Info("Ajout réalisé avec succès")	// CORRECTION orthographique
            FIN
