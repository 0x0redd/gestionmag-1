        type_code : 4
        p_codes :
         -
           code : |1-
            
           type : 14
         -
           code : |1-
            // --- Récupérer l'ID du receiving à utiliser ---
            SI PAS HExécuteRequête(REQ_select_max_id_receivings) ALORS
            	Erreur("Impossible de récupérer l'ID du receiving : " + HErreurInfo())
            	RETOUR
            FIN
            
            // CORRECTION : Tester le résultat de HLitPremier
            SI HLitPremier(REQ_select_max_id_receivings) = Faux ALORS
            	Erreur("Aucun ID de réception trouvé.")
            	RETOUR
            FIN
            
            nReceiving_ID	est un entier	= REQ_select_max_id_receivings.le_maximum_id
            // CORRECTION : Supprimer la redéclaration de nItemID (déjà déclaré plus haut)
            // nItemID		est un entier	= COMBO_Article
            
            // --- Initialisation des montants ---
            nTotalSaisi		est un réel		= 0
            
            // --- Paiement par chèque ---
            SI cheque_saisie <> "" ALORS
            	REQ_Insert_Payment_Type2.Parampayment_type		= "Chèque"
            	REQ_Insert_Payment_Type2.Parampayment_amount	= Val(cheque_saisie)
            	REQ_Insert_Payment_Type2.Paramreceivings_id		= nReceiving_ID
            	REQ_Insert_Payment_Type2.Paramdate_cheque		= SAI_Date_cheque
            	REQ_Insert_Payment_Type2.Paramcreated_at		= DateSys()
            
            	SI PAS HExécuteRequête(REQ_Insert_Payment_Type2) ALORS
            		Erreur("Erreur lors du paiement par chèque : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	nTotalSaisi += Val(cheque_saisie)
            FIN
            
            // --- Paiement en espèce ---
            SI espece_saisie <> "" ALORS
            	REQ_Insert_Payment_Type2.Parampayment_type		= "Espèce"
            	REQ_Insert_Payment_Type2.Parampayment_amount	= Val(espece_saisie)
            	REQ_Insert_Payment_Type2.Paramreceivings_id		= nReceiving_ID
            	REQ_Insert_Payment_Type2.Paramdate_cheque		= Null
            	REQ_Insert_Payment_Type2.Paramcreated_at		= DateSys()
            
            	SI PAS HExécuteRequête(REQ_Insert_Payment_Type2) ALORS
            		Erreur("Erreur lors du paiement en espèce : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	nTotalSaisi += Val(espece_saisie)
            FIN
            
            // --- Paiement par carte ---
            SI carte_saisie <> "" ALORS
            	REQ_Insert_Payment_Type2.Parampayment_type		= "Carte"
            	REQ_Insert_Payment_Type2.Parampayment_amount	= Val(carte_saisie)
            	REQ_Insert_Payment_Type2.Paramreceivings_id		= nReceiving_ID
            	REQ_Insert_Payment_Type2.Paramdate_cheque		= Null
            	REQ_Insert_Payment_Type2.Paramcreated_at		= DateSys()
            
            	SI PAS HExécuteRequête(REQ_Insert_Payment_Type2) ALORS
            		Erreur("Erreur lors du paiement par carte : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	nTotalSaisi += Val(carte_saisie)
            FIN
            
            // --- Vérification du montant total ---
            nMontantTotal est un réel = Val(total_achat)
            
            SI nTotalSaisi >= nMontantTotal ALORS
            	// Paiement complet
            	REQ_receiving_payment_status.Paramid						= nReceiving_ID
            	REQ_receiving_payment_status.ParamreceivingPayment_status	= "payée"
            
            	SI PAS HExécuteRequête(REQ_receiving_payment_status) ALORS
            		Erreur("Erreur lors de la mise à jour du statut de paiement : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	Info("Réception et paiement enregistrés avec succès. Statut : Payée.")
            SINON
            	// Paiement incomplet
            	nMontantRestant est un réel = nMontantTotal - nTotalSaisi
            
            	REQ_receiving_payment_status.Paramid						= nReceiving_ID
            	REQ_receiving_payment_status.ParamreceivingPayment_status	= "non complet"
            
            	SI PAS HExécuteRequête(REQ_receiving_payment_status) ALORS
            		Erreur("Erreur lors de la mise à jour du statut de paiement : " + HErreurInfo())
            		RETOUR
            	FIN
            
            	Info("Paiement incomplet. Montant restant : " + nMontantRestant + " DA")
            FIN
