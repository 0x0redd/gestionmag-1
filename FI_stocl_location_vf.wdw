            // Récupérer l'ID du produit sélectionné (valeur mémorisée)
            idArticle est un entier = COMBO_Article
            
            // Vérifier que l'ID est valide
            SI idArticle <> 0 ALORS
            
            	// Aller lire l'article correspondant
            	SI HLitRecherche(items, id, idArticle) ALORS
            
            		// Afficher le nom et le prix dans les libellés
            		Produit_name	= items.name
            		prix_produit	= items.cost_price
            		IMG__977806_3475758_removebg_preview_1 = items.pictures
            
            		// Générer une référence unique à partir de la date et l'ID produit
            		sDate		est une chaîne		= DateVersChaîne(DateSys(), "AAAAMMJJ")
            		sIDProduit	est une chaîne		= NumériqueVersChaîne(idArticle, "04")	// Format 4 chiffres
            
            		reference = "REF-" + sIDProduit
            
            	SINON
            		Info("Article introuvable")
            	FIN
            
            SINON
            	Info("Aucun article sélectionné")
            FIN
