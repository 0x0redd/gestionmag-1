      -
        code : |1-
         ParentFenêtre est un champ
         
         
         SI PAS HExécuteRequête(REQ_select_articles) ALORS
         	RETOUR
         FIN
         
         // Lire les résultats
         HLitPremier(REQ_select_articles)
         
         bProduitCritique est un booléen = Faux
         
         
         
         // Réinitialiser la zone répétée
         ZR_SansNom1 = []
         TANTQUE PAS HEnDehors()
         	SI REQ_select_articles.quantity <= REQ_select_articles.seuil_critique ALORS
         		// Ajouter une ligne dans la zone répétée
         		nIndice est un entier = TableAjouteLigne(ZR_SansNom1)
         		ZR_SansNom1[nIndice].LIB_Article	= REQ_select_articles.name
         		ZR_SansNom1[nIndice].LIB_Qte		= REQ_select_articles.quantity
         		bProduitCritique					= Vrai
         	FIN
         	HLitSuivant(REQ_select_articles)
         FIN
         
         SI bProduitCritique ALORS
         	ZR_SansNom1..Visible	= Vrai
         	LIB_Attention..Visible	= Vrai
         FIN
