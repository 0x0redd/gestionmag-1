       name : BTN_Button
       identifier : 0x28261ddd15549484
       internal_properties : 
       properties :
        x : 1174
        y : 565
        width : 116
        height : 36
        cursor_id : 32649
        accelerator_key : 65535
        tab_order : 21
       style : {}
