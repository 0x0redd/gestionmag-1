     p_codes :
      -
        code : |1-
         // =========================================
         // CODE POUR LE BOUTON "Modifier"
         // =========================================
         
         // =========================================
         // 1. Lecture des champs de l'interface
         // =========================================
         nReceivingID		est un entier	= Receiving_id
         nItemID				est un entier	= COMBO_Article
         nStockID			est un entier	= COMBO_Stock..Valeur
         nItemIDs			est un entier	= ItemID
         nStockIDs			est un entier	= StockID
         nFournisseurID		est un entier	= COMBO_Fournisseur
         sPaymentStatus		est une chaîne	= COMBO_payment_statuts..ValeurAffichée
         nQuantite			est un entier	= Val(Quantitée)
         rTotal				est un réel		= Val(total_achat)
         rPriceArticle		est un réel		= Val(price_article)
         
         // =========================================
         // 2. Variables pour la gestion des stocks
         // =========================================
         nAncienneQuantite	est un entier	= 0
         nStockActuel		est un entier	= 0
         nNouveauStock		est un entier	= 0
         nDifference			est un entier	= 0
         
         // =========================================
         // 3. Validation des données obligatoires
         // =========================================
         SI nReceivingID = 0 OU nItemID = 0 OU nStockID = 0 ALORS
         	Erreur("Veuillez sélectionner tous les champs obligatoires (Article, Stock, Fournisseur)")
         	RETOUR
         FIN
         
         SI nQuantite <= 0 ALORS
         	Erreur("La quantité doit être supérieure à 0")
         	RETOUR
         FIN
         
         // =========================================
         // 4. Récupérer l'ancienne quantité achetée
         // =========================================
         REQ_get_old_quantity.Paramreceiving_id = nReceivingID
         
         SI HExécuteRequête(REQ_get_old_quantity) ALORS
         	SI HLitPremier(REQ_get_old_quantity) ALORS
         		nAncienneQuantite = REQ_get_old_quantity.quantity_purchased
         	SINON
         		Erreur("Impossible de récupérer les données de réception")
         		RETOUR
         	FIN
         SINON
         	Erreur("Erreur lors de la récupération de l'ancienne quantité : " + HErreurInfo())
         	RETOUR
         FIN
         
         // =========================================
         // 5. Récupérer le stock actuel
         // =========================================
         REQ_select_stock_reel.Paramitem_id		= nItemIDs
         REQ_select_stock_reel.Paramlocation_id	= nStockIDs
         
         SI HExécuteRequête(REQ_select_stock_reel) ALORS
         	SI HLitPremier(REQ_select_stock_reel) ALORS
         		nStockActuel = REQ_select_stock_reel.quantity
         		Info("Stock actuel : " + nStockActuel)
         	SINON
         		Erreur("Aucun stock trouvé pour cet article à cet emplacement")
         		RETOUR
         	FIN
         SINON
         	Erreur("Erreur lors de la récupération du stock : " + HErreurInfo())
         	RETOUR
         FIN
         
         // =========================================
         // 6. Calcul du nouveau stock
         // =========================================
         // Si on augmente la quantité achetée, on ajoute au stock
         // Si on diminue la quantité achetée, on retire du stock
         SI nQuantite > nAncienneQuantite ALORS
         	// Augmentation de la quantité achetée
         	nDifference		= nQuantite - nAncienneQuantite
         	nNouveauStock	= nStockActuel + nDifference
         	Info("Augmentation de " + nDifference + " unités")
         SINON SI nQuantite < nAncienneQuantite ALORS
         	// Diminution de la quantité achetée
         	nDifference		= nAncienneQuantite - nQuantite
         	nNouveauStock	= nStockActuel - nDifference
         	Info("Diminution de " + nDifference + " unités")
         SINON
         	// Pas de changement de quantité
         	nNouveauStock = nStockActuel
         	Info("Aucun changement de quantité")
         FIN
         
         // =========================================
         // 7. Vérification du stock suffisant
         // =========================================
         SI nNouveauStock < 0 ALORS
         	Erreur("Stock insuffisant !" + RC + ...
         	"Stock actuel : " + nStockActuel + RC + ...
         	"Quantité à retirer : " + nDifference + RC + ...
         	"Stock résultant serait : " + nNouveauStock)
         	RETOUR
         FIN
         
         // =========================================
         // 8. Confirmation avant modification
         // =========================================
         sMessage est une chaîne = "Confirmer la modification ?" + RC + RC + ...
         "Ancienne quantité : " + nAncienneQuantite + RC + ...
         "Nouvelle quantité : " + nQuantite + RC + RC + ...
         "Stock actuel : " + nStockActuel + RC + ...
         "Nouveau stock : " + nNouveauStock
         
         SI OuiNon(Vrai, sMessage) = Non ALORS
         	RETOUR
         FIN
         
         // =========================================
         // 9. Début de transaction
         // =========================================
         HTransactionDébut()
         
         // =========================================
         // 10. Mise à jour de la table receiving_items
         // =========================================
         REQ_modif_receiving_item.Paramreceivings_id			= nReceivingID
         REQ_modif_receiving_item.Paramitem_id				= nItemID
         REQ_modif_receiving_item.Paramitem_location			= nStockID
         REQ_modif_receiving_item.Paramquantity_purchased	= nQuantite
         REQ_modif_receiving_item.Paramreceiving_quantity	= nQuantite
         
         SI HExécuteRequête(REQ_modif_receiving_item) = Faux ALORS
         	HTransactionAnnule()
         	Erreur("Erreur lors de la mise à jour des articles reçus : " + HErreurInfo())
         	RETOUR
         FIN
         
         // =========================================
         // 11. Mise à jour de la table receivings
         // =========================================
         REQ_modif_receivings.Paramid						= nReceivingID
         REQ_modif_receivings.ParamreceivingPayment_status	= sPaymentStatus
         REQ_modif_receivings.Paramsupplier_id				= nFournisseurID
         
         SI HExécuteRequête(REQ_modif_receivings) = Faux ALORS
         	HTransactionAnnule()
         	Erreur("Erreur lors de la mise à jour de la réception : " + HErreurInfo())
         	RETOUR
         FIN
         
         // =========================================
         // 12. Mise à jour du stock (item_quantities)
         // =========================================
         REQ_modif_item_quantities3.ParamItemID		= nItemID
         REQ_modif_item_quantities3.ParamLocationID	= nStockID
         REQ_modif_item_quantities3.ParamNewQuantity	= nNouveauStock
         
         SI HExécuteRequête(REQ_modif_item_quantities3) = Faux ALORS
         	HTransactionAnnule()
         	Erreur("Erreur lors de la mise à jour du stock : " + HErreurInfo())
         	RETOUR
         FIN
         
         // Fermer la fenêtre
         MaFenêtre.Ferme()
