         // Récupérer l'ID du produit sélectionné dans le ComboBox
         idArticle est un entier = COMBO_Article
         
         // Vérifier que l'article est bien sélectionné
         SI idArticle <> 0 ALORS
         
         	// Lire l'enregistrement correspondant dans le fichier 'items'
         	SI HLitRecherche(items, id, idArticle) ALORS
         		price_article							= items.cost_price
         		// Fonction qui se déclenche lorsque l'utilisateur saisit la quantité
