      -
        code : |1+
         // =========================================
         // Récupération des valeurs depuis les champs
         // =========================================
         nReceivingID	est un entier	= Receiving_id
         
         nItemID			est un entier	= COMBO_Article
         nStockID		est un entier	= COMBO_Stock
         nFournisseurID	est un entier	= COMBO_Fournisseur
         nPaymentType	est une chaîne	= COMBO_Type_De_paiement..ValeurAffichée
         sPaymentStatus	est une chaîne	= COMBO_payment_statuts..ValeurAffichée
         nQuantite		est un entier	= Val(Quantitée)
         rTotal			est un réel		= Val(total_achat)
         price_article	est un réel		= Val(price_article)	// Assure-toi de récupérer le prix quelque part
         
         
         // =========================================
         // 1. UPDATE receivings
         // =========================================
         
         REQ_modif_receivings.ParamreceivingPayment_status= sPaymentStatus
         REQ_modif_receivings.Paramsupplier_id = nFournisseurID
         REQ_modif_receivings.paramid = nReceivingID
         
         // Exécuter la requête avec le paramètre
         SI HExécuteRequête(REQ_modif_receivings) = Faux ALORS
         	Erreur("Erreur mise à jour dans 'receivings' : " + HErreurInfo())
         	RETOUR
         FIN
         
         
         // =========================================
         // 2. UPDATE receiving_items
         // =========================================
         
         REQ_modif_receiving_item.Paramitem_id						= nItemID
         REQ_modif_receiving_item.Paramitem_location					= nStockID
         REQ_modif_receiving_item.Paramreceiving_quantity			= nQuantite
         REQ_modif_receiving_item.Paramquantity_purchased			= nQuantite
         REQ_modif_receiving_item.Paramitem_cost_price				= price_article
         REQ_modif_receiving_item.Paramreceivings_id					= nReceivingID
         
         // Exécution sans paramètre si la requête ne l'attend pas
         SI HExécuteRequête(REQ_modif_receiving_item) = Faux ALORS
         	Erreur("Erreur mise à jour dans 'receivings' : " + HErreurInfo())
         	RETOUR
         FIN
         
         // =========================================
         // 3. UPDATE payment_type
         // =========================================
         REQ_modif_payment_type.Paramreceivings_id		= nReceivingID
         REQ_modif_payment_type.Parampayment_type		= nPaymentType
         
         SI HExécuteRequête(REQ_modif_payment_type) = Faux ALORS
         	Erreur("Erreur mise à jour dans 'payment_type' : " + HErreurInfo())
         	RETOUR
         FIN
         
         // =========================================
         // 4. UPDATE item_quantities (facultatif)
         // =========================================
         REQ_modif_item_quantities.Paramitem_id		= nItemID
         REQ_modif_item_quantities.Paramlocation_id	= nStockID
         REQ_modif_item_quantities.Paramquantity		= nQuantite
         
         SI HExécuteRequête(REQ_modif_item_quantities) = Faux ALORS
         	Erreur("Erreur mise à jour dans 'item_quantities' : " + HErreurInfo())
         	RETOUR
         FIN
         
         
         
         Info("Mise à jour réussie !")
         MaFenêtre.Ferme()
