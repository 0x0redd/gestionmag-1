     -
       name : BTN_Supprimer
       identifier : 0x2847163b287e7b36
       internal_properties : 
       properties :
        x : 1024
        y : 19
        width : 146
        height : 61
        anchor : 4
        accelerator_key : 65535
        tab_order : 4
       style : {}
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1+
            // Bouton Supprimer - Suppression d'une vente (cascade)
            
            // 1. Vérifier la sélection
            SI TableSelect(TABLE_liste_entré_ventes_h) = -1 ALORS
            	Erreur("Veuillez sélectionner une vente à supprimer.")
            	RETOUR
            FIN
            
            // 2. Récupération des données
            nLigne		est un entier		= TableSelect(TABLE_liste_entré_ventes_h)
            
            nSaleID		est un entier		= TABLE_liste_entré_ventes_h[nLigne].COL_ID
            nItemID		est un entier		= TABLE_liste_entré_ventes_h[nLigne].COL_item_id
            nLocationID	est un entier		= TABLE_liste_entré_ventes_h[nLigne].COL_stock_id
            
            // 3. Confirmation
            SI OuiNon("Confirmer la suppression de cette vente ?", "Cette action supprimera tous les paiements, articles et quantités associées.") = Faux ALORS
            	RETOUR
            FIN
            
            // 4. Suppression en cascade
            
            // 4.1 Supprimer les paiements
            REQ_supprimer_sales_payment.Paramsale_id = nSaleID
            SI PAS HExécuteRequête(REQ_supprimer_sales_payment) ALORS
            	Erreur("Erreur suppression paiements : " + HErreurInfo())
            	RETOUR
            FIN
            
            // 4.2 Supprimer les articles de vente
            REQ_supprimer_sales_items.Paramsale_id = nSaleID
            SI PAS HExécuteRequête(REQ_supprimer_sales_items) ALORS
            	Erreur("Erreur suppression des articles : " + HErreurInfo())
            	RETOUR
            FIN
            
            // 4.3 Supprimer les quantités (optionnel si la vente affecte le stock)
            REQ_supprimer_item_quantities.Paramitem_id		= nItemID
            REQ_supprimer_item_quantities.Paramlocation_id	= nLocationID
            SI PAS HExécuteRequête(REQ_supprimer_item_quantities) ALORS
            	Erreur("Erreur suppression des quantités : " + HErreurInfo())
            	RETOUR
            FIN
            
            // 4.4 Supprimer la vente principale
            REQ_supprimer_sales.Paramid = nSaleID
            SI PAS HExécuteRequête(REQ_supprimer_sales) ALORS
            	Erreur("Erreur suppression de la vente : " + HErreurInfo())
            	RETOUR
            FIN
            
            // 5. Actualisation
            TableSupprime(TABLE_liste_entré_ventes_h, nLigne)
            Info("Vente supprimée avec succès.")
            
            HExécuteRequête(REQ_liste_entré_ventes_Y)
            TableAffiche(TABLE_liste_entré_ventes_h, taInit)
            
