       style : {}
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1-
            // Vérifier qu'une ligne est sélectionnée
            nLigneSelectionnee est un entier = TableSelect(TABLE_liste_entré_ventes_h)
            SI nLigneSelectionnee = -1 ALORS
            	Info("Veuillez sélectionner une ligne.")
            	RETOUR
            FIN
            
            // Vérifier que la ligne existe dans la table
            SI nLigneSelectionnee < 1 OU nLigneSelectionnee > TableOccurrence(TABLE_liste_entré_ventes_h) ALORS
            	Info("Erreur : ligne sélectionnée invalide.")
            	RETOUR
            FIN
            
            // Récupérer les valeurs avec vérification
            sales_id	est un entier	= TABLE_liste_entré_ventes_h[nLigneSelectionnee]
            location_id	est un entier	= TABLE_liste_entré_ventes_h[nLigneSelectionnee].COL_stock_id
            articles_id	est un entier	= TABLE_liste_entré_ventes_h[nLigneSelectionnee].COL_item_id
            
            // Vérifier que les valeurs sont valides
            SI sales_id <= 0 ALORS
            	Info("Erreur : ID de vente invalide.")
            	RETOUR
            FIN
            
            SI location_id <= 0 ALORS
            	Info("Erreur : ID de stock invalide.")
            	RETOUR
            FIN
            
            SI articles_id <= 0 ALORS
            	Info("Erreur : ID d'article invalide.")
            	RETOUR
            FIN
            
            // Ouvrir la fenêtre
            Ouvre(FEN_fen_update_vente_H, sales_id, location_id, articles_id)
