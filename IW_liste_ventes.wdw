     type_code : 70
     p_codes :
      -
        code : |1+
         PROCÉDURE MyWindow()
         SI HExécuteRequête(REQ_select_vente_liste) ALORS
         	// Vider la table visuelle avant de la remplir
         	TableSupprimeTout(TABLE_liste_entré_ventes_h)
         	// Boucle pour ajouter chaque ligne de la requête à la table
         	POUR TOUT REQ_select_vente_liste
         		// Gestion des valeurs nulles avec opérateur "??" (valeur par défaut)
         		TableAjouteLigne(TABLE_liste_entré_ventes_h, ...
