  procedures :
   -
     name : InitModification
     procedure_id : 1736766186180109265
     type_code : 14
     code : |1+
      PROCEDURE InitModification()
      
      // Rechercher l'utilisateur
      SI PAS HLitRecherche(users, id, gIdUtilisateur) ALORS
      	Info("Utilisateur introuvable.")
      	RETOUR
      FIN
      
      gIdPersonne = users.person_id
      
      // Rechercher les infos de la personne
      SI PAS HLitRecherche(people, id, gIdPersonne) ALORS
      	Info("Personne associée introuvable.")
      	RETOUR
      FIN
      
      // Remplir les champs
      EDT_Prenom		= people.first_name
      EDT_Nom			= people.last_name
      SAI_Gender		= people.gender
      EDT_Telephone	= people.phone_number
      SAI_Email		= people.email
      EDT_Adresse		= people.address_1
      EDT_Ville		= people.city
      EDT_Pays		= people.country
      EDT_Password	= ""
      
      // Charger le rôle
      SI HLitRecherchePremier(role_user, user_id, gIdUtilisateur) ALORS
      	SI HLitRecherche(roles, id, role_user.role_id) ALORS
      		COMBO_Role = roles.name
      	FIN
      FIN
      
     type : 458752
   -
     name : ValiderModification
     procedure_id : 1736768393793551202
     type_code : 14
     code : |1+
      
      PROCÉDURE ValiderModification()
      
      // Sécurité : Vérifier que les IDs sont valides
      SI gIdUtilisateur = 0 OU gIdPersonne = 0 ALORS
      	Info("Aucun utilisateur sélectionné.")
      	RETOUR
      FIN
      
      // Étape 1 : Mise à jour des informations dans la table people
      SI PAS HLitRecherche(people, id, gIdPersonne) ALORS
      	Info("Impossible de trouver la personne liée à l'utilisateur.")
      	RETOUR
      FIN
      
      people.first_name	= EDT_Prenom
      people.last_name	= EDT_Nom
      people.gender		= SAI_Gender
      people.phone_number	= EDT_Telephone
      people.email		= SAI_Email
      people.address_1	= EDT_Adresse
      people.city			= EDT_Ville
      people.country		= EDT_Pays
      people.updated_at	= DateSys()
      
      SI HModifie(people) = Faux ALORS
      	Info("Erreur lors de la mise à jour des informations personnelles.")
      	RETOUR
      FIN
      
      // Étape 2 : Mise à jour des informations dans la table users
      SI PAS HLitRecherche(users, id, gIdUtilisateur) ALORS
      	Info("Impossible de trouver l'utilisateur.")
      	RETOUR
      FIN
      
      users.name			= EDT_Prenom + " " + EDT_Nom
      users.email			= SAI_Email
      users.updated_at	= DateSys()
      
      // Si un mot de passe est saisi, le hacher et le mettre à jour
      SI EDT_Password <> "" ALORS
      	HashBuffer est un Buffer = HashChaîne(HA_SHA_256, EDT_Password)
      	users.password = BufferVersHexa(HashBuffer)
      FIN
      
      SI HModifie(users) = Faux ALORS
      	Info("Erreur lors de la mise à jour de l'utilisateur.")
      	RETOUR
      FIN
      
      // Étape 3 : Mise à jour du rôle dans role_user
      // On supprime d'abord l'ancien rôle (s'il existe)
      SI HLitRecherche(role_user, user_id, gIdUtilisateur) ALORS
      	HSupprime(role_user)
      FIN
      
      // Identifier le rôle sélectionné
      roleId est un entier
      
      SI COMBO_Role..ValeurAffichée = "SuperAdmin" ALORS
      	roleId = 1
      SINON SI COMBO_Role..ValeurAffichée = "Responsable" ALORS
      	roleId = 2
      SINON SI COMBO_Role..ValeurAffichée = "Magasinier" ALORS
      	roleId = 3
      SINON
      	Info("Rôle sélectionné invalide.")
      	RETOUR
      FIN
      
      
      
      // Vérifier que le rôle existe dans la table roles
      SI PAS HLitRecherche(roles, id, roleId) ALORS
      	Info("Ce rôle n'existe pas.")
      	RETOUR
      FIN
      
      // Ajouter le nouveau rôle
      HReset(role_user)
      role_user.user_id	= gIdUtilisateur
      role_user.role_id	= roleId
      
      SI HAjoute(role_user) = Faux ALORS
      	Info("Erreur lors de l'attribution du rôle.")
      	RETOUR
      FIN
      
      // ✅ Succès
      Info("Utilisateur modifié avec succès.")
      Ferme()
      
     type : 458752
   -
     name : OuvreFenetre
     procedure_id : 1736776734621130160
     type_code : 14
     code : |1+
      PROCÉDURE OuvreFenetre(nIDUtilisateur est un entier)
      
      gIdUtilisateur = nIDUtilisateur
      
      // Charger l'utilisateur
      SI PAS HLitRecherche(users, id, gIdUtilisateur) ALORS
      	Info("Utilisateur introuvable.")
      	RETOUR
      FIN
      
      // Charger la personne liée
      gIdPersonne = users.person_id
      SI PAS HLitRecherche(people, id, gIdPersonne) ALORS
      	Info("Données personnelles introuvables.")
      	RETOUR
      FIN
      
      // Remplir les champs
      EDT_Prenom		= people.first_name
      EDT_Nom			= people.last_name
      SAI_Gender		= people.gender
      EDT_Telephone	= people.phone_number
      SAI_Email		= users.email
      EDT_Adresse		= people.address_1
      EDT_Ville		= people.city
      EDT_Pays		= people.country
      EDT_Password	= "" // vide pour ne pas réafficher le hash
      
      // Déterminer le rôle actuel
      SI HLitRecherche(role_user, user_id, gIdUtilisateur) ALORS
      	SI HLitRecherche(roles, id, role_user.role_id) ALORS
      		COMBO_Role = roles.name
      	FIN
      FIN
      
      
      
